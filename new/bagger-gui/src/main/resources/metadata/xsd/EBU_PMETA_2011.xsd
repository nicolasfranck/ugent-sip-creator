<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:pmeta="urn:ebu:metadata-schema:ebupmeta_2011"
	targetNamespace="urn:ebu:metadata-schema:ebupmeta_2011" elementFormDefault="qualified"
	attributeFormDefault="unqualified">
	<import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="ebu_p_meta_att.xsd"/>

	<!-- IMPORTANT NOTE: This new version pf the P-META schema has a new namespace compliant with RFC5174 (EBU namespace). The definition of
classification scheme element and structures are now part of a separate EBU specification-->

	<!--**************************************************************************************************************************-->
	<!-- ************************************************BASIC TYPES********************************************************* -->
	<!-- ************************************************************************************************************************** -->
	<simpleType name="AspectRatioType">
		<restriction base="string">
			<pattern value="([0-9])+[:]([0-9])+"/>
		</restriction>
	</simpleType>
	<element name="CurrencyAmount" type="pmeta:CurrencyAmountType"/>
	<simpleType name="CurrencyAmountType">
		<restriction base="decimal">
			<fractionDigits value="2"/>
		</restriction>
	</simpleType>
	<element name="ElapsedTime" type="duration">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The elapsed time from the beginning of the programme/item/media
				object to the beginning of an event specified by the set containing the attribute.
					<H4>Aliases</H4>Duration <H4>Example</H4>PT0H5M13.18S for an elapsed time of 5
				minutes, 13 seconds and 18/100 of a second. <H4>ReferenceData</H4>ISO 8601, RFC3339
			</documentation>
		</annotation>
	</element>
	<complexType name="TimeMarkType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A complex type to define a time mark to identify the beginning
				or the end of a segment. This time reference can expressed in three different forms:
				a time duration from the beginning of the material, a timecode, or as a number of
				elapsed edit units</documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:Timecode" minOccurs="0"/>
			<element ref="pmeta:ElapsedTime" minOccurs="0"/>
			<element ref="pmeta:ElapsedUnits" minOccurs="0"/>
		</sequence>
	</complexType>
	<element name="ElapsedUnits" type="pmeta:ElapsedUnitsType"/>
	<complexType name="ElapsedUnitsType">
		<annotation>
			<documentation>Number of edit units representing the fraction of time calculated
				contextually as an inverse to the frame rate for video or sample rate for audio
			</documentation>
		</annotation>
		<simpleContent>
			<extension base="integer">
				<attribute name="editUnit" type="float">
					<annotation>
						<documentation>duration of edit unit in seconds</documentation>
					</annotation>
				</attribute>
			</extension>
		</simpleContent>
	</complexType>
	<element name="SegmentStart" type="pmeta:TimeMarkType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A time mark to identify the beginning of a
				segment</documentation>
		</annotation>
	</element>
	<element name="SegmentEnd" type="pmeta:TimeMarkType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A time mark to identify the end of a segment</documentation>
		</annotation>
	</element>
	<simpleType name="TimecodeType">
		<restriction base="string">
			<pattern
				value="(([0-1][0-9])|([2][0-3])):[0-5][0-9]:[0-5][0-9](([.,])|([:;]))[0-2][0-9]"/>
		</restriction>
	</simpleType>
	<element name="Timecode" type="pmeta:TimecodeType"/>
	<simpleType name="FacsimileNumberType">
		<restriction base="string">
			<pattern value="[+]/d{15}"/>
		</restriction>
	</simpleType>
	<simpleType name="TelephoneNumberType">
		<restriction base="string">
			<pattern value="[+]/d{15}"/>
		</restriction>
	</simpleType>


	<!--********************************************************** -->
	<!--*****CLASSIFICATION************************************************ -->
	<!--*********************************************************** -->


	<complexType name="TextualType">
		<simpleContent>
			<extension base="string">
				<attribute ref="xml:lang" use="optional"/>
			</extension>
		</simpleContent>
	</complexType>


	<!--********************************************************** -->
	<!--*****ELEMENTS OF SIMPLE AND COMPLEX TYPES************************************************ -->
	<!--*********************************************************** -->
	<element name="AccountHolder" type="pmeta:ContributorDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4> The details of the holder (a person or organisation) of an
				account within a financial institution. <H4>Example</H4>EBU. </documentation>
		</annotation>
	</element>
	<element name="AccountName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The name associated with an account held by an organisation (or
				a person) within a financial institution. <H4>Example</H4>BBC WORLDWIDE LTD STERLING
				ACCOUNT </documentation>
		</annotation>
	</element>
	<element name="AccountNumber" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The identifying number provided by a financial institution for
				use by an organisation (or a person) in specifying a particular account held within
				the financial institution. </documentation>
		</annotation>
	</element>
	<element name="Address" type="pmeta:AddressType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>An element to describe the details of a person or organisation.
					<H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="AddressCountyStateName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The county (or, where appropriate, state or district) name in a
				postal address. <H4>Example</H4> Berkshire, Idaho, Manitoba. <H4>External
					reference</H4>OpenSMEF Attribute - PAD_COUNTY_STATE_NAME </documentation>
		</annotation>
	</element>
	<element name="AddressDeliveryCode" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Delivery location code used by the relevant national postal
				service(s). The nature and format of the code varies considerably between different
				countries. Note that some countries now include a character ahead of their internal
				postal code to indicate the country. <H4>Aliases</H4>Post Code, ZIP Code
					<H4>Example</H4>(country indicated within parenthesis) W1A 1AA (United Kingdom);
				SE23 3NL (United Kingdom); 10135 (Italy); SE-105 (Sweden); D-80939 (Germany); N-0340
				(Norway); 55101 (USA); R3N 1S8 (Canada) <H4>ReferenceData</H4>National Postal
				Service of each country. </documentation>
		</annotation>
	</element>
	<element name="AddressElectronicName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>An electronic mail address (e-mail)
				<H4>Example</H4>contact@ebu.ch </documentation>
		</annotation>
	</element>
	<element name="AddressFacsimileNumber" type="pmeta:FacsimileNumberType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A facsimile machine number, the precise nature and/or context of
				which will be defined by the set containing the number. <H4>Example</H4>020 8534
				1234 </documentation>
		</annotation>
	</element>
	<element name="AddressLineName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A line in the postal address. This might be a street name, a
				house name, a house number, a district of a city, etc. <H4>Example</H4>No. 45;
				Crowthorn Road; Ocean Boulevard; Rue de Montreal </documentation>
		</annotation>
	</element>
	<element name="AddressTelephoneNumber" type="pmeta:TelephoneNumberType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A telephone number, the precise nature and/or context of which
				will be defined by the set containing the number. <H4>Example</H4>020 8534 1234
			</documentation>
		</annotation>
	</element>
	<element name="AddressTownCityName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The town or city in a postal address. <H4>Example</H4>Paris;
				Glasgow; Minneapolis . </documentation>
		</annotation>
	</element>
	<element name="AddressWebAddress" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>An address of a resource on the world-wide web in the form of a
				URL or an http. <H4>Example</H4>www.ebu.ch </documentation>
		</annotation>
	</element>
	<element name="AudienceRatingPercentage" type="decimal">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The percentage of the TV private household population or
				sub-population (as estimated from the sample) viewing the event across its duration,
				averaged across the minutes which comprise that period. <H4>Example</H4>46.3
					<H4>ReferenceData</H4>EBU Escort 2.4, Escort 2006 </documentation>
		</annotation>
	</element>
	<element name="AudienceReachPercentage" type="decimal">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The cumulative percentage of the TV private household population
				or sub-population (as estimated from the sample) viewing the event across its
				duration, cumulated across the minutes which comprise that period.
				<H4>Example</H4>68.2 <H4>ReferenceData</H4>EBU Escort 2.4, Escort 2006
			</documentation>
		</annotation>
	</element>
	<element name="AudienceScoreRecordingTechnique" type="pmeta:AudienceScoreRecordingTechniqueType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies by a code and/or its name the method used to measure
				the audience score </documentation>
		</annotation>
	</element>
	<element name="AudienceScoreRecordingTechniqueCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A controlled code for the technique of recording the audience
				score for events.
				<H4>Example</H4>urn:ebu:metadata:cs:AudienceRecordingTechniqueCodeCS:1;
				www.ebu.ch/metadata/cs/AudienceRecordingTechniqueCodeCS#1
				<H4>ReferenceData</H4>AudienceScoreRecordingTechniqueCodeCS</documentation>
		</annotation>
	</element>
	<element name="AudienceScoreRecordingTechniqueName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The name of the technique of recording the audience score for
				events. <H4>Example</H4>poll, people meter. </documentation>
		</annotation>
	</element>
	<element name="AudienceSharePercentage" type="decimal">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The amount of viewing obtained by the event expressed as a
				percentage of all TV viewing across the period of time of the event.
					<H4>Example</H4>70 <H4>ReferenceData</H4>EBU Escort 2.4, Escort 2006
			</documentation>
		</annotation>
	</element>
	<element name="AudioBitrate" type="nonNegativeInteger">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Specifies the bitrate (in bits per second).
				<H4>Aliases</H4>audio average bitrate; audio compression bitrate
				<H4>Example</H4>44100, 48000 <H4>ReferenceData</H4>SMPTE MDD 04 02 03 01 02 00 00 00
			</documentation>
		</annotation>
	</element>
	<element name="AudioBitsPerSample" type="string">
		<!-- change to integer-->
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The maximum number of significant bits for encoding a sample.
					<H4>Aliases</H4>audio resolution; bits per sample <H4>Example</H4>16; 20; 24;
					<H4>ReferenceData</H4>AudioBitsPerSampleCS </documentation>
		</annotation>
	</element>
	<element name="AudioCompression" type="pmeta:AudioCompressionType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>An element to identify an audio compression scheme by an
				identification code and/or its name. </documentation>
		</annotation>
	</element>
	<element name="AudioCompressionCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The controlled code identifying any compression scheme, noise
				reduction scheme, or other non-linear processing applied to an audio signal.
					<H4>ReferenceData</H4>AudioCompressionCodeCS </documentation>
		</annotation>
	</element>
	<element name="AudioCompressionName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The name for any compression scheme, noise reduction scheme, or
				other non-linear processing applied to an audio signal. <H4>Example</H4>MPEG Layer
				II; Dolby A; IEC pre-emphasis. </documentation>
		</annotation>
	</element>
	<element name="AudioFixedBitrateIndicator" type="boolean">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A flag indicating whether the coding standard delivers a fixed
				bitrate to the decoding/playback 24 Attribute Definition List device. ’True’
				indicates a fixed bitrate. </documentation>
		</annotation>
	</element>
	<element name="AudioFormat" type="pmeta:AudioFormatType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>An element to describe the format of content of the event .
			</documentation>
		</annotation>
	</element>
	<element name="AudioFormatCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A controlled code for a sound format type.
				<H4>Example</H4>stereo, mono, etc. <H4>ReferenceData</H4>AudioFormatCodeCS
			</documentation>
		</annotation>
	</element>
	<element name="AudioFormatName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The name of the sound format type e.g. mono, 2 channel stereo, 5
				channel surround sound, etc. <H4>Example</H4>Mono; Stereo; 5 Channel surround sound
			</documentation>
		</annotation>
	</element>
	<element name="AudioLoudness" type="float">
		<annotation>
			<documentation xml:lang="EN"> The integrated loudness over the duration of a programme
				-Programme Loudness Level is the value (in LUFS) of ProgrammeLoudness. LUFS is the
				Loudness Unit, referenced to Full Scale. ‘LUFS’ is equivalent to ‘LKFS’ (which is
				used in ITU-R BS.1770-1). According to EBU Recommendation R128, the Programme
				Loudness Level shall be normalised to a Target Level of -23 LUFS. The permitted
				deviation from the Target Level shall generally not exceed ±1 LU for programmes
				where an exact normalisation to Target Level is not achievable practically (for
				example, live programmes). </documentation>
		</annotation>
	</element>
	<element name="AudioLoudnessRange" type="float">
		<annotation>
			<documentation xml:lang="EN"> Loudness Range (LRA) describes the distribution of
				loudness within a programme and shall be measured with a meter compliant with EBU
				Tech Doc 3342. </documentation>
		</annotation>
	</element>
	<element name="AudioMaximumTruePeakLevel" type="float">
		<annotation>
			<documentation xml:lang="EN">It is the maximum value of the audio signal waveform of a
				programme in the continuous time domain. The Maximum Permitted True Peak Level of a
				programme during production shall be -1 dBTP (dB True Peak), measured with a meter
				compliant with both ITU-R BS.1770 and EBU Tech Doc 3341. </documentation>
		</annotation>
	</element>
	<element name="AudioReferenceLevel" type="integer">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The audio level in Dbm for 0VU. <H4>ReferenceData</H4>SMPTE MDD
				RP210 A 04 02 01 01 03 00 00 00 </documentation>
		</annotation>
	</element>
	<element name="AudioSampleRate" type="float">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Specifies the rate of sampling in kHz used to create the audio
				clip instance in samples per second. <H4>Enumerated List</H4>11, 20.050, 32 44.100,
				48, 96. </documentation>
		</annotation>
	</element>
	<element name="AvailableExchangeFormat" type="pmeta:TechnicalDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The technical details of the format in which content is
				available for exchange. </documentation>
		</annotation>
	</element>
	<element name="Award" type="pmeta:AwardType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The type of an award presented to a person, programme, series,
				etc. <H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="AwardName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The name of an award presented to a person, programme, series,
				etc. and signifying public approbation, professional achievement, etc.
					<H4>Example</H4>The Silver Rose of Montreux </documentation>
		</annotation>
	</element>
	<element name="BankAccountDetails" type="pmeta:BankAccountDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides detailed information on accounts per account holder.
			</documentation>
		</annotation>
	</element>
	<element name="BankDetails" type="pmeta:OrganisationDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Information concerning the financial institution
					<H4>Example</H4>Bank Of Credit, Panama </documentation>
		</annotation>
	</element>
	<element name="BrandTitle" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Title of a Brand, which is a collection of assets with a
				recognisable collective identity. <H4>Example</H4>BBC Drama; BBC Sport.
			</documentation>
		</annotation>
	</element>
	<element name="BrandTitleHistory" type="pmeta:BrandTitleHistoryType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Recapitulates the titles attributed to a piece of content during
				its lifetime. </documentation>
		</annotation>
	</element>
	<element name="Classification" type="pmeta:ClassificationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides a data structure for classification information related
				to a programme group. a programme or an item, etc. <H4>Example</H4>genre, format,
				targeted audience, content alert </documentation>
		</annotation>
	</element>
	<element name="ClassificationData" type="pmeta:ClassificationTermType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Instantiates a content concept by its code or name
					<H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ClassificationDimension" type="pmeta:ClassificationDimensionType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a subcategory within a classification scheme. A
				particular section of classification provided as an independent set of reference
				data or within an existing set of reference data, and identified by its code or
				name. <H4>Example</H4>E.g. the Sport category in ContentGenreCS </documentation>
		</annotation>
	</element>
	<element name="ClassificationDimensionCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A code identifying a classification dimension within a
				multidimensional classification scheme for audio-visual works, according to its
				specifications. <H4>Example</H4><H4>ReferenceData</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ClassificationDimensionName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The name of a classification dimension within a multidimensional
				classification scheme for audio-visual works. <H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ClassificationSchemeIdentification"
		type="pmeta:ClassificationSchemeIdentificationType">
		<!-- add identification to reserve the ClassificationScheme name for CSs-->
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a set of reference data defined to characterise
				content like genre of a particular programme. The scheme is identifier by its code
				or name. <H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ClassificationSchemeCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A code identifying a registered classification scheme for
				audio-visual works. <H4>Example</H4>EBU P/META, TV-Anytime, MPEG-7, Dublin Core,
				PBCore, DVB, EBU ESCORT2.4, EBU ESCORT 2006, etc.
				<H4>ReferenceData</H4>ConentAlertSchemeCodeCS </documentation>
		</annotation>
	</element>
	<element name="ClassificationSchemeName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The name of a classification scheme for audio-visual works.
					<H4>Example</H4>ContentGenreCS </documentation>
		</annotation>
	</element>
	<element name="ClassificationTerm" type="pmeta:ClassificationTermTypeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A code identifying a classification term within a classification
				scheme for audio-visual works, according to its specifications. <H4>Example</H4>
				<H4>ReferenceData</H4>ContentGenreCS </documentation>
		</annotation>
	</element>
	<element name="ClassificationTermCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A code identifying a classification term within a classification
				scheme for audio-visual works, according to its specifications. <H4>Example</H4>
				<H4>ReferenceData</H4>tva:AtmosphereCS, tva:ContentAlertCS, tva:ContentCommercialCS,
				ebu:ContentGenreCS, tva:ContentCS, ebu:IPTCSubjectCodeCS, ebu:EscortCS,
				ebu:IntentionCodeCS, tva:IntentionCS, tva:IntendedAudienceCS,
				ebu:IntendedAudienceCodeCS, ebu:EditorialFormatCodeCS, tva:EditorialFormatCS,
				ebu:ReferenceAttributeCS, (user defined - template only), ebu:ProgrammeGroupTypeCS
			</documentation>
		</annotation>
	</element>
	<element name="ClassificationTermName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The name of a classification term within a classification scheme
				for audio-visual works. <H4>Example</H4>
				<H4>ReferenceData</H4>Infotainment; Western; Horror; Language Learning.
			</documentation>
		</annotation>
	</element>
	<element name="ClassifiedSubject" type="pmeta:ClassificationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides a subject extracted from a thesaurus <H4>Example</H4>
				<H4>ReferenceData</H4>ContentCS </documentation>
		</annotation>
	</element>
	<element name="ClassifiedSubjectContext" type="pmeta:ClassificationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides a context extracted from a thesaurus <H4>Example</H4>
				<H4>ReferenceData</H4>SubjectContextCS </documentation>
		</annotation>
	</element>
	<element name="ColourCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A code specifying the editorial colour description for the
				content being exchanged. <H4>Example</H4>
				<H4>ReferenceData</H4>ColourCodeCS </documentation>
		</annotation>
	</element>
	<element name="Contacts" type="pmeta:ContributionType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Contacts refer a primary contact (to a person or organisation)
				or a public relations agency for an actor/actress, or any personnel supporting the
				person. <H4>Example</H4>Secretaries, make-up artists, hairdressers, and personal
				assistants, etc. </documentation>
		</annotation>
	</element>
	<element name="ContractClausesAndRightsList" type="pmeta:ContractClausesAndRightsListType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides a list of rights and contract clauses </documentation>
		</annotation>
	</element>
	<element name="ContractClauseDescription" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Description of a clause within a contract. <H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ContractDate" type="date">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The date at which a contract has been issued
				<H4>Example</H4>2007-06-09 </documentation>
		</annotation>
	</element>
	<element name="ContractDetails" type="pmeta:ContractDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Communicates all the details of a contract/agreement between two
				parties for the use/exploitation of one, or more, media assets. </documentation>
		</annotation>
	</element>
	<element name="ContractFeesDetails" type="pmeta:ContractFeesDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Communicates the details of fees related to the contract .
			</documentation>
		</annotation>
	</element>
	<element name="ContractingParties" type="pmeta:ContributionType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies key parties involved in a contract
				<H4>Example</H4>Contractor </documentation>
		</annotation>
	</element>
	<element name="ContractLine" type="pmeta:ContractLineType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Communicates a particularly relevant line of the contract .
					<H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ContractLineAmount" type="pmeta:CurrencyAmountDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The financial value associated to this line of a contract
					<H4>Example</H4>EU100 </documentation>
		</annotation>
	</element>
	<element name="ContractLineName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A free text description for the service or product covered by
				the contract line. </documentation>
		</annotation>
	</element>
	<element name="ContractLineNumber" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>An identifier number for the contract line within a contract.
					<H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ContractNumber" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Number assigned to the contract for the transaction by the
				issuer of the contract for the purposes of identifying the contract.
					<H4>Example</H4>LB00313201/1; LB00313236/01 </documentation>
		</annotation>
	</element>
	<element name="ContractPaymentInstalmentAmount" type="pmeta:CurrencyAmountDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Where contracts/agreements allow for payment by instalment this
				attribute identifies the amount of money due in a specified instalment.
					<H4>Example</H4>GBP10000 </documentation>
		</annotation>
	</element>
	<element name="ContractPaymentInstalmentCount" type="nonNegativeInteger">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Where contracts/agreements allow for payment by instalment this
				attribute identifies the particular instalment in the sequence of payments (e.g.
				instalment 1, instalment 5, etc.). <H4>Example</H4>1. 4. 6. 23 </documentation>
		</annotation>
	</element>
	<element name="ContractPaymentInstalmentDueDate" type="pmeta:TimestampType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Where contracts/agreements allow for payment by instalment this
				attribute identifies the date by which the particular instalment in the sequence of
				payments (e.g. instalment 1, instalment 5, etc.) is due. The date is inclusive in
				that the payment may be made on the date quoted. <H4>Example</H4>2007-02-05
			</documentation>
		</annotation>
	</element>
	<element name="ContractTermsOfBusinessDescription" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>This is a free text definition of the terms of business which
				apply to a specified contract or transaction, defined and/or specified by the
				organisation issuing the contract. These will vary from organisation to organisation
				and, perhaps, within organisations from transaction to transaction. </documentation>
		</annotation>
	</element>
	<element name="ContractTotalCost" type="pmeta:CurrencyAmountDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Determines the overall amount to be paid for the entire contract
				. <H4>Example</H4>USD15000 </documentation>
		</annotation>
	</element>
	<element name="ContractType" type="pmeta:ContractTypeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defines a type of contract through a code and/or name.
					<H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ContractTypeCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A controlled code identifying the type of contract.
					<H4>Example</H4>
				<H4>ReferenceData</H4>ContractTypeCodeCS </documentation>
		</annotation>
	</element>
	<element name="ContractTypeName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a contract type recognised between people and/or
				organisations involved in the life cycle of media-assets . <H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ContractualMaterial" type="pmeta:IdentityMinimumDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies the content material covered by the contract.
					<H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="Contribution" type="pmeta:ContributionType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Describes a contribution by associating person or organisation
				details to a role <H4>Example</H4>Creator: RAI, Rome </documentation>
		</annotation>
	</element>
	<element name="ContributorDetails" type="pmeta:ContributorDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Brings additional information about a contributor.
					<H4>Example</H4>A person or organisation details. </documentation>
		</annotation>
	</element>
	<element name="CopyrightAgent" type="pmeta:ContributorDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Person(s) or organisation(s) who (at the date/time otherwise
				defined) are the copyright agents for the people or organisations who hold copyright
				in some, or all, of the related material. <H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="CopyrightHolder" type="pmeta:ContributorDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Person(s) or organisation(s) who (at the date/time otherwise
				defined) hold copyright in some, or all, of the related material . <H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="Country" type="pmeta:CountryType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a country by its ISO code or name. </documentation>
		</annotation>
	</element>
	<element name="CountryCode" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Internationally agreed code for code for a specified country.
					<H4>Example</H4>FR, CH, IT, GB, HU, etc.
				<H4>ReferenceData</H4>ebu:Iso3166CountryCodeCS </documentation>
		</annotation>
	</element>
	<element name="CountryName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Name of a country <H4>Example</H4>France, Germany, Greece,
				Italy, Spain, United Kingdom. </documentation>
		</annotation>
	</element>
	<element name="CoveredTerritory" type="pmeta:TerritoryType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The country covered e.g. by the rights or the transmission is
				identified by its code and / or name. <H4>Example</H4>
				<H4>ReferenceData</H4>ISO 3166-1 </documentation>
		</annotation>
	</element>
	<element name="Currency" type="pmeta:CurrencyType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a currency by its code or name. </documentation>
		</annotation>
	</element>
	<!--	<element name="CurrencyAmountPerLine" type="pmeta:CurrencyAmountDetailsType">
	</element-->
	<element name="CurrencyCode" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>An internationally agreed code for a currency.
				<H4>Example</H4>USD, EURO, GBP, CHF <H4>ReferenceData</H4>ebu:Iso4217CurrencyCodeCS
			</documentation>
		</annotation>
	</element>
	<element name="CurrencyName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The name of an internationally recognised currency
					<H4>Example</H4>Euro, US Dollar, Swiss Franc, etc. <H4>ReferenceData</H4>ISO
				4217 </documentation>
		</annotation>
	</element>
	<element name="DataCompression" type="pmeta:DataCompressionType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies the scheme used to compress data by its name or code.
			</documentation>
		</annotation>
	</element>
	<element name="DataCompressionCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A controlled code for the type of processing applied to a data
				object in order to achieve data compression, but also file grouping and data
				encoding. <H4>Example</H4> 9.1.6 DAI_COMPRESSION_CODE
				<H4>ReferenceData</H4>DataCompressionCodeCS, IANA MIME TYPES </documentation>
		</annotation>
	</element>
	<element name="DataCompressionName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4> A textual name for the type of processing applied to a data
				object in order to achieve data compression, but also file grouping and data
				encoding. <H4>Example</H4>
				<H4>ReferenceData</H4>EBU P/META, IANA MIME TYPES </documentation>
		</annotation>
	</element>
	<element name="DataDefaultLanguage" type="pmeta:LanguageType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defines by its code or name the language used in the set and
				associated subset instance unless otherwise specified. <H4>ReferenceData</H4>ISO
				3166-1 </documentation>
		</annotation>
	</element>
	<element name="DefaultReferenceScheme" type="pmeta:ClassificationSchemeIdentificationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies the classification system used by default.
					<H4>ReferenceData</H4>ClassificationSchemeCS </documentation>
		</annotation>
	</element>
	<element name="DeviceModelName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The model name as applied to a device by the manufacturer and by
				which the device is known. <H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="DeviceNumber" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The identifier allocated by an organisation to a device. This is
				additional to the manufacturer’s serial number. </documentation>
		</annotation>
	</element>
	<element name="DeviceSerialNumber" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Serial number applied to a particular instance of a device by
				its manufacturer. </documentation>
		</annotation>
	</element>
	<element name="DeviceType" type="pmeta:DeviceTypeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Attributes a type of device using a code or name. Device types
				are defined to the level of identifying the technology used by the device without
				going down to the level of a complete technical specification of an individual
				device. </documentation>
		</annotation>
	</element>
	<element name="DeviceTypeCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A controlled code defining a device type. <H4>Example</H4>
				<H4>ReferenceData</H4>DeviceTypeCodeCS </documentation>
		</annotation>
	</element>
	<element name="DeviceTypeName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A name characterising a type of device. <H4>Example</H4>audio
				mixer, microphone, router, tape recorder, Minidisc Recorder , camera, audio tape
				machine, digital linear tape, random access memory (RAM), display, etc.
					<H4>ReferenceData</H4>EBU DeviceTypeCS </documentation>
		</annotation>
	</element>
	<element name="DubbingSubtitlingGranted" type="pmeta:LanguageType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defines the language in which dubbing rights have been granted.
			</documentation>
		</annotation>
	</element>
	<element name="Duration" type="duration">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The duration of a particular version of a programme or item, at
				the frame rate it is intended to be shown at, measured in hours, minutes, seconds,
				and decimal fractions of a second. This may be derivable from the duration of its
				constituent media objects. <H4>Example</H4>"P3Y6M4DT12H30M0S" defines "a period of
				three years, six months, four days, twelve hours, thirty minutes, and zero seconds".
				. <H4>ReferenceData</H4>ISO 8601 </documentation>
		</annotation>
	</element>
	<element name="EditorialControl" type="pmeta:EditorialControlType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides an editorial classification of a programme using codes
				or textual attributes <H4>Aliases</H4>Concept, genre, etc </documentation>
		</annotation>
	</element>
	<element name="EditorialControlCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Classification of a programme, or programme segment, or group of
				programmes, according to its content or subject. This may also apply to publication
				services. <H4>Example</H4>e.g. a ’sport’ channel.
				<H4>ReferenceData</H4>EditorialControlCodeCS </documentation>
		</annotation>
	</element>
	<element name="EditorialControlDetails" type="pmeta:EditorialControlDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides information on who has control over the editorial
				process. </documentation>
		</annotation>
	</element>
	<element name="EditorialControlName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Classification of a programme, or programme segment, or group of
				programmes, according to its content or subject. This may also apply to publication
				services. <H4>Example</H4>Daily News; Religious Philosophies; Show Business;
				Medicine, etc. <H4>ReferenceData</H4>EBU, ESCORT 2.4, ESCORT 2006, TV-Anytime,
				MPEG-7, etc. ContentGenreCS, etc. </documentation>
		</annotation>
	</element>
	<element name="EquipmentDetails" type="pmeta:EquipmentDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides information about a piece of equipment </documentation>
		</annotation>
	</element>
	<element name="EventEnd" type="pmeta:TimestampType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The date (GMT/UTC) and time at the end of an event specified by
				the set containing the attribute. </documentation>
		</annotation>
	</element>
	<element name="EventStart" type="pmeta:TimestampType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Gives the date and time of the beginning of an event
			</documentation>
		</annotation>
	</element>
	<element name="ExcludedTerritory" type="pmeta:TerritoryType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defines a list of excluded territories (country, region, etc.)
				e.g. regarding the application of particular rights within a contract clause.
				Territories are identified by a code and /or name. </documentation>
		</annotation>
	</element>
	<element name="ExplanatoryNote" type="pmeta:ExplanatoryNoteType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Any required explanatory information as contextually required.
			</documentation>
		</annotation>
	</element>
	<element name="ExternalSchemeExchangeSet" type="pmeta:ExternalSchemeExchangeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Uses external reference classification and a particular a term
				(to which a value can be attributed) within a default or reference classification
				scheme. <H4>Aliases</H4>Classification </documentation>
		</annotation>
	</element>
	<element name="FestivalName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The name of a festival or award ceremony
				<H4>Example</H4>Festival de Cannes, The Academy Awards, Mostra del Cinema di
				Venezia, Golden Globe, Montreux, Avoriaz, Berlin, etc. </documentation>
		</annotation>
	</element>
	<element name="FileFormatType" type="pmeta:FileFormatTypeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a type of file by its code and/or name.
			</documentation>
		</annotation>
	</element>
	<element name="FileFormatTypeCode" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A controlled code communicating the format of a computer file.
				The value shall be a pair "content type"/"content sub-type" registered at IANA, as
				defined by IETF RFC 2045 and 2046. <H4>Example</H4>text/plain; text/rtf;
				audio/basic; image/jpeg ; image/png; video/mpeg; video/quicktime; application/pdf;
				application/postscript <H4>ReferenceData</H4>ebu:FileFormatCS </documentation>
		</annotation>
	</element>
	<element name="FileFormatTypeName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The name of the format of a computer file. <H4>Example</H4>MXF;
				Broadcast Wave; Mpeg Transport Stream; PDF </documentation>
		</annotation>
	</element>
	<element name="FileInstance" type="pmeta:FileInstanceType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Describes the main characteristics of a file being exchanged.
			</documentation>
		</annotation>
	</element>
	<element name="FilePathName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The name of the file path to a complete digital media, data,
				metadata etc file, that will be unique within a file server or a storage medium.
					<H4>Aliases</H4>
				<H4>Example</H4> /storage_A/PMeta/export/relevantmaterial.avi
				<H4>ReferenceData</H4>SMPTE MDD, etc. </documentation>
		</annotation>
	</element>
	<element name="FileSize" type="nonNegativeInteger">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The number of bytes of a computer file. <H4>Example</H4>1235673
			</documentation>
		</annotation>
	</element>
	<element name="FirstEpisodeIdentifier" type="pmeta:IdentifierDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies the first episode in a programme group (series)
			</documentation>
		</annotation>
	</element>
	<element name="FirstPublicationEventStart" type="pmeta:TimestampType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defines the date and time of a first publication.
					<H4>Aliases</H4>Release date and time </documentation>
		</annotation>
	</element>
	<element name="GrantOfRightsConditions" type="pmeta:GrantOfRightsConditionsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Describes the conditions under which rights are granted.
					<H4>Aliases</H4>Usage restrictions </documentation>
		</annotation>
	</element>
	<element name="GrantOfRightsDetails" type="pmeta:GrantOfRightsDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Describes the rights for a particular content or group of
				content. </documentation>
		</annotation>
	</element>
	<element name="GraphicUsageType" type="pmeta:GraphicUsageTypeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defines a type of graphic usage by its code and/or name.
			</documentation>
		</annotation>
	</element>
	<element name="GraphicUsageTypeCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A controlled code for the type of usage to which a graphic is
				put. <H4>Example</H4>
				<H4>ReferenceData</H4>GraphicUsageTypeCodeCS </documentation>
		</annotation>
	</element>
	<element name="GraphicUsageTypeName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A type of usage to which a graphic is put. <H4>Example</H4>title
				sequence; graphic overlay, etc. </documentation>
		</annotation>
	</element>
	<element name="Identifier" type="pmeta:IdentifierDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A contextually defined identifier. An identifier can take
				different forms, which type is determined by an name or code. Finally, the
				identifier value itself. </documentation>
		</annotation>
	</element>
	<element name="IdentifierNumber" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A unique identifier assigned to the programme group, programme,
				item, media object, or an instance of one of those by the producing organisation for
				identification and other purposes. <H4>Aliases</H4>Production Number, Programme
				Number <H4>Example</H4>ABH521021A <H4>ReferenceData</H4>TV-Anytime CRID, ISO ISAN,
				SMPTE UMID, SMPTE UPID, etc. See also organisation and storage identifiers
			</documentation>
		</annotation>
	</element>
	<element name="IdentifierType" type="pmeta:IdentifierTypeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Determines the type of an identifier by its code or name.
			</documentation>
		</annotation>
	</element>
	<element name="IdentifierTypeCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A code identifying the type of identifier being exchanged.
					<H4>Example</H4>
				<H4>ReferenceData</H4>IdentifierTypeCodeCS for audio-visual content,
				OrganisationIdentifierTypeCodeCS for organisations, StorageIdentifierTypeCode for
				storage. </documentation>
		</annotation>
	</element>
	<element name="IdentifierTypeName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A code identifying the type of identifier being exchanged
					<H4>Example</H4>
				<H4>ReferenceData</H4>ISAN, V-ISAN, UMID, UPID, CRID, etc. </documentation>
		</annotation>
	</element>
	<element name="IdentityMinimumDetails" type="pmeta:IdentityMinimumDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides minimum information necessary to identify the material.
			</documentation>
		</annotation>
	</element>
	<element name="IndexExternalScheme" type="pmeta:ClassificationSchemeIdentificationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies an external indexed reference classification scheme
				by its code and/or name. <H4>Aliases</H4>Classification Scheme </documentation>
		</annotation>
	</element>
	<element name="IndexExternalSchemeTerm" type="pmeta:IndexExternalSchemeTermType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defines a term from an external indexed classification scheme.
					<H4>Aliases</H4>Classification term </documentation>
		</annotation>
	</element>
	<element name="IndexExternalSchemeTermScope" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>When a descriptive term taken from an external (i.e. non-EBU)
				description scheme is used this free-text attribute may contain information further
				defining the scope of the term used. <H4>Aliases</H4>Explanatory Note
					<H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="IssuedBy" type="pmeta:ContributorDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identification of a contributor (person or organisation) who has
				issued e.g. a list of available material, or responded to a request.
			</documentation>
		</annotation>
	</element>
	<!--	<element name="InstalmentAmount" type="pmeta:CurrencyAmountDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Where contracts/agreements allow for payment by instalment this attribute identifies the amount of money due in a specified instalment. 
				<H4>Example</H4>GBP 10000
			</documentation>
		</annotation>
	</element -->
	<element name="Item" type="pmeta:ItemType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies and describe a content item. <H4>Aliases</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ItemDetails" type="pmeta:ItemDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides detailed information about a content item
			</documentation>
		</annotation>
	</element>
	<element name="ItemDescription" type="pmeta:ItemDescriptionType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides descriptive information about a content item
			</documentation>
		</annotation>
	</element>
	<element name="ItemEndScriptCueText" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4> Text which provides the cue for the end of an item. Typically,
				this will be spoken dialogue by a performer or presenter within the item, but it
				could refer to another action such as a stage direction or piece of music which
				marks the end of the item. </documentation>
		</annotation>
	</element>
	<element name="ItemIdentification" type="pmeta:ItemIdentificationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies an individual content item. <H4>Aliases</H4>Item Id
			</documentation>
		</annotation>
	</element>
	<element name="ItemInformation" type="pmeta:ItemInformationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides additional information on the use of a content item.
			</documentation>
		</annotation>
	</element>
	<element name="ItemScript" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A text script for a programme item. This may be a complete
				production script or it may only contain the spoken text. It differs, therefore,
				from a media object script which is a description of a single media object in terms
				applicable to that media object type. <H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ItemScriptSceneNumber" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The scene number which identifies the position of the scene, in
				relation to others in the production, within the shooting script.
				<H4>Aliases</H4>Scene Number <H4>Example</H4>1; 34; 2a; 3c.
				<H4>ReferenceData</H4>SMPTE RP210 Key 01 05 06 00 00 00 00 00 </documentation>
		</annotation>
	</element>
	<element name="ItemScriptSceneTakeCount" type="nonNegativeInteger">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>An incrementing number identifying the particular take of a
				specified scene within a production. <H4>Aliases</H4>Take Count
				<H4>Example</H4>1,2,3.. <H4>ReferenceData</H4>SMPTE RP210 Key 01 05 07 00 00 00 00
				00 </documentation>
		</annotation>
	</element>
	<element name="ItemSequenceNumber" type="nonNegativeInteger">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>An integer indicating where, in the sequence of items forming a
				programme, a particular item is to be found. <H4>Aliases</H4>
				<H4>Example</H4>1, 10... </documentation>
		</annotation>
	</element>
	<element name="ItemsGroup" type="pmeta:ItemsGroupType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defines a group of content items </documentation>
		</annotation>
	</element>
	<element name="ItemSoundTypeName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A textual description of the type of sound used in the item.
					<H4>Example</H4>Silent; Natural; Part narrated; </documentation>
		</annotation>
	</element>
	<element name="ItemStartScriptCueText" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Text which provides the cue for the start of an item. Typically,
				this will be spoken dialogue by a performer or presenter, but it could refer to
				another action such as a stage direction or piece of music which marks the beginning
				of the item. </documentation>
		</annotation>
	</element>
	<element name="ItemSubtitle" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A secondary title given to the Item in complement to the item
				title. </documentation>
		</annotation>
	</element>
	<element name="ItemSynopsis" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides the synopsis of the content item. <H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ItemTitle" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4> The title given to an item (constituent editorial part of a
				programme) by its producer. <H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ItemTitleHistory" type="pmeta:ItemTitleHistoryType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Recapitulates the history of titles attributed to a content item
				during its lifetime. </documentation>
		</annotation>
	</element>
	<element name="KeywordName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Important word or phrase from a title, a document, the material
				description, or other used as an index to content.. <H4>Example</H4>Louis XIV;
				Economy; Brasil; Health Service; Defence. </documentation>
		</annotation>
	</element>
	<element name="Keywords" type="pmeta:KeywordsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Important word or phrase from a title, a document, the material
				description, or other used as an index to content.. </documentation>
		</annotation>
	</element>
	<element name="Language" type="pmeta:LanguageType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defines a language by its name and / or code. </documentation>
		</annotation>
	</element>
	<element name="LanguageCode" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Internationally agreed code for a specified language.
					<H4>Aliases</H4>ISO International Language Code (2-character or 3-character set)
					<H4>Example</H4>FR, GB, HU <H4>ReferenceData</H4>ebu:Iso639_1LanguageCodeCS,
				ebu:Iso639_2LanguageCodeCS </documentation>
		</annotation>
	</element>
	<element name="LanguageHistory" type="pmeta:LanguageHistoryType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Recapitulates the languages used in association with the content
				under different circumstances during its lifetime. </documentation>
		</annotation>
	</element>
	<element name="LanguageName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Gives a language name <H4>Example</H4>Dutch, English, French,
				German, Italian, etc. </documentation>
		</annotation>
	</element>
	<element name="LanguageUsage" type="pmeta:LanguageUsageType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Describes the contextual usage of a particular language.
			</documentation>
		</annotation>
	</element>
	<element name="LanguageUsageCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A code to indicate how a particular language is used in a
				particular context. <H4>Example</H4>Commentator language, original spoken dialogue,
				subtitle language, dubbed language, etc. <H4>ReferenceData</H4>LanguageUsageCodeCS
			</documentation>
		</annotation>
	</element>
	<element name="LanguageUsageDescription" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A free form description of how a particular language is used in
				a particular context. <H4>Aliases</H4>Explanatory Note <H4>Example</H4>Subtitle is
				used to help in comprehension of strongly accented speaker. </documentation>
		</annotation>
	</element>
	<element name="LicenseCoveredMaterial" type="pmeta:IdentityMinimumDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides minimum identification and description information of
				content material covered by a license. </documentation>
		</annotation>
	</element>
	<element name="Licensee" type="pmeta:ContributorDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Person or organisation being granted rights through a license by
				a third party (Licensor). </documentation>
		</annotation>
	</element>
	<element name="LicenseeSignatory" type="pmeta:PersonDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides information on the person who signed a contract on
				behalf of a licensee. <H4>Aliases</H4>
			</documentation>
		</annotation>
	</element>
	<element name="Licensor" type="pmeta:ContributorDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Person or organisation granting rights through a license to a
				third party (Licensee). </documentation>
		</annotation>
	</element>
	<element name="LicensorSignatory" type="pmeta:PersonDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides information on the person who signed a contract on
				behalf of a licensor. <H4>Aliases</H4>
			</documentation>
		</annotation>
	</element>
	<element name="LiveActionCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Indicates whether a production features live action, animation
				or a mixture of both. <H4>Aliases</H4>Origination Code <H4>Example</H4>
				<H4>ReferenceData</H4>ebu:LiveActionCodeCS </documentation>
		</annotation>
	</element>
	<element name="Location" type="pmeta:LocationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides different location related information for the content
				being described. </documentation>
		</annotation>
	</element>
	<element name="LocationActionName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The name of the location of the captured (or to be captured)
				action. For example, if St Paul’s Cathedral was filmed from the top of the BT Tower
				then the LocationActionName would be St. Paul’s Cathedral. Not to be confused with
				the name of the place that the real location is standing in for (see
				LocationSettingName). <H4>Example</H4>BT Tower, Eiffel Tower, Northern Ireland
				border near Dundalk </documentation>
		</annotation>
	</element>
	<element name="LocationCaptureName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The name of the location of the sensor (e.g. camera) used in the
				production of content. For example, if St Paul’s Cathedral was filmed from the top
				of the BT Tower then the LocationCaptureName would be the BT Tower. Not to be
				confused with the name of the place that the real location is standing in for (see
				LocationSettingName). <H4>Aliases</H4>Shooting location <H4>Example</H4>BT Tower,
				Eiffel Tower, Northern Ireland border near Dundalk. </documentation>
		</annotation>
	</element>
	<element name="LocationSettingName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The dramatic setting of a piece of content. For example, a play
				set in Moscow might be shot in Dundee. In this case, although the material was
				filmed in Dundee the LocationSettingName would be Moscow. The setting name might
				include more than just place information. It might, for example, define historical
				period. In the case above, the setting might have been Moscow, 1930s. The setting
				might also be completely fictional. An example of this would be a setting of
				’Utopia’ in a dramatisation of Sir Thomas More’s book or ’An Outer Circle of Hell’
				in an adaptation of Dante’s "Inferno". <H4>Aliases</H4>
				<H4>Example</H4>Paris 1789; Quartic; Naples, Beppe’s Coffee shop </documentation>
		</annotation>
	</element>
	<element name="Material" type="pmeta:MaterialType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides information on the availability of the material and its
				relation to a group of programme. </documentation>
		</annotation>
	</element>
	<element name="MaterialCaptureDate" type="pmeta:TimestampType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The date of capture of a shooting material
				<H4>Aliases</H4>Shooting date <H4>Example</H4>2007-04-12 <H4>ReferenceData</H4>ISO
				8601 </documentation>
		</annotation>
	</element>
	<element name="MaterialExchangeByServiceInstance"
		type="pmeta:MaterialExchangeByServiceInstanceType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Describes an exchange via a transmission service (and not as a
				file or on a storage device as otherwise covered by MaterialExchangeInstanceType'
				StorageDetails. </documentation>
		</annotation>
	</element>
	<element name="MaterialExchangeInstance" type="pmeta:MaterialExchangeInstanceType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies and describe an instance of the material as it has
				been exchanged. </documentation>
		</annotation>
	</element>
	<element name="MaterialExchangeService" type="pmeta:ServiceType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a service through which an exchange is performed.
			</documentation>
		</annotation>
	</element>
	<element name="MaterialExchangeStart" type="pmeta:TimestampType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies the date and time when an exchange actually starts
			</documentation>
		</annotation>
	</element>
	<element name="MaterialRelationshipsDetails" type="pmeta:MaterialRelationshipsDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defines the relationship(s) of the material to a brand, a
				programme group, a programme, an item, or a media object through a relationship code
				defining the type of relation(see reference data) and brand, programme group,
				programme, item, or media object identification. </documentation>
		</annotation>
	</element>
	<element name="MaterialRelationshipTypeCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Specifies the type of relationship existing between materials
				identified in association with this attribute in a superset.. <H4>Aliases</H4>How
				related <H4>Example</H4>Is Trailer Of <H4>ReferenceData</H4>tva:HowRelatedCS
			</documentation>
		</annotation>
	</element>
	<element name="MaterialReleaseForPublicationDate" type="pmeta:TimestampType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides a date of availability of the material for publication
				release. <H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="MediaObject" type="pmeta:MediaObjectType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies and describes a media object .
				<H4>Aliases</H4>Component </documentation>
		</annotation>
	</element>
	<element name="MediaObjectCapturedDescription" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Describes a captured media object. <H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="MediaObjectDescription" type="pmeta:MediaObjectDescriptionType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>An itemised description of a media object. </documentation>
		</annotation>
	</element>
	<element name="MediaObjectDetails" type="pmeta:MediaObjectDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides detailed information on a media object </documentation>
		</annotation>
	</element>
	<element name="MediaObjectTrackIdentifier" type="pmeta:IdentifierDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies the track containing the media object instance during
				an exchange. This can be used, when applicable, for exchange by a physical medium
				(e.g. a Video Tape) as well as by a transmission link. </documentation>
		</annotation>
	</element>
	<element name="MediaObjectIdentification" type="pmeta:MediaObjectIdentificationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides minimum information for the identification of a media
				object. </documentation>
		</annotation>
	</element>
	<element name="MediaObjectInformation" type="pmeta:MediaObjectInformationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides information on a media object </documentation>
		</annotation>
	</element>
	<element name="MediaObjectScript" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides a description of the media object as originally
				intended in the Script”, before realisation / instantiation (how the media object
				should be). <H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="MediaObjectTitle" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The title of a media object which is a single content element
				(only one medium). <H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="MediaObjectTitleHistory" type="pmeta:MediaObjectTitleHistoryType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Recapitulates the history of titles attributed to a media object
				during its lifetime. </documentation>
		</annotation>
	</element>
	<element name="MediaObjectTypeCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>This is the type of media object being exchanged. e.g. SHOT;
				GRAPHIC; DATA; STILL; AUDIO <H4>Example</H4>
				<H4>ReferenceData</H4>MediaTypeCS </documentation>
		</annotation>
	</element>
	<element name="OrganisationDescription" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Brings additional descriptive information about an organisation
					<H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="OrganisationDetails" type="pmeta:OrganisationDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Brings detailed information on an organisation. </documentation>
		</annotation>
	</element>
	<element name="OrganisationIdentifier" type="pmeta:IdentifierDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies an organisation uniquely by an identification number.
					<H4>Example</H4>
				<H4>ReferenceData</H4>OrganisationIdentifierTypeCode shall be used as
				IdentifierTypeCode in IdentifierDetails </documentation>
		</annotation>
	</element>
	<element name="OrganisationName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The name of an organisation, that is any grouping of
				individuals, company or company structure with whom there is an association to carry
				out tasks or responsibilities for the development, usage or management of media
				assets. <H4>Aliases</H4>
				<H4>Example</H4>BBC Media Data Group; RAI; SMPTE; World Health Organisation, EBU
			</documentation>
		</annotation>
	</element>
	<element name="OrganisationType" type="pmeta:OrganisationTypeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies the type of organisation by a code or name.
			</documentation>
		</annotation>
	</element>
	<element name="OrganisationTypeCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A codified value which indicates an organisation’s type. This is
				stated in terms of the organisation’s constitution. E.g. whether it is an internal
				unit of another organisation, a limited company, charity, co-operative and so on.
					<H4>Example</H4>
				<H4>ReferenceData</H4>OrganisationTypeCodeCS </documentation>
		</annotation>
	</element>
	<element name="OrganisationTypeName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The name of a particular type of organisation.
				<H4>Example</H4>Limited Company, Partnership, Co-operative, Charity, Internal
				Division. </documentation>
		</annotation>
	</element>
	<element name="OriginationCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A controlled code identifying the (EBU defined) process sector
				from which the material was originated. <H4>Example</H4>
				<H4>ReferenceData</H4>ebu:EscortCS, tva:OriginationCodeCS </documentation>
		</annotation>
	</element>
	<element name="PersonDescription" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A textual description of a person used, where necessary, to
				distinguish them from another person of the same name. <H4>Example</H4>Member of
				Scottish Parliament; Dentist; Poet; Historian; American farmer. </documentation>
		</annotation>
	</element>
	<element name="PersonDetails" type="pmeta:PersonDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides additional information on a person. </documentation>
		</annotation>
	</element>
	<element name="PersonFirstName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The first minor name of the person. In the western european
				tradition this would actually be the person’s first name. For example, in the
				western European name John Smith Jordan this would be John. You would not expect
				this name to be the primary determinant of the persons position on an alphabetically
				ordered list. <H4>Aliases</H4>Given name, Christian name <H4>Example</H4>Andreas,
				Laurent, Alberto, Marteen, etc. </documentation>
		</annotation>
	</element>
	<element name="PersonIdentifier" type="pmeta:IdentifierDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a person. </documentation>
		</annotation>
	</element>
	<element name="PersonMiddleName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The secondary minor name(s) of the person. For example, in the
				Western European name John Smith Jordan this would be Smith. You would not expect
				this name to be the primary determinant of the persons position on an alphabetically
				ordered list. <H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="PersonLastName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The person’s main name. You would expect this name to be the
				primary determinant of the persons position on an alphabetically ordered list. In
				the Western European tradition, and many others, this would be the person’s surname
				or family name. In the name John Smith Jordan the main name is Jordan. This field,
				however, is also used for names which do not fall into the, typically, western model
				of first name, middle name(s), and last name. <H4>Aliases</H4>Surname; Family Name
					<H4>Example</H4>Jordan, Boch, Messina, Carter, etc. </documentation>
		</annotation>
	</element>
	<element name="PersonSalutationShortForm" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A shortened salutation as applied to a person’s name.
					<H4>Example</H4>Mr.; Miss; Mrs.; Ms; Lord, etc. </documentation>
		</annotation>
	</element>
	<element name="PersonStageName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The name by which a performer is known for professional purposes
				(e.g. Reginald Dwight performs under the name Elton John). This may be all, or part
				of, their real name or it could be another name entirely. A stage name may comprise
				of a number of parts but is represented here as a single string. It may also
				sometimes include a salutation. <H4>Example</H4>Sting; Cher; Terence Hill; Buster
				Keaton, etc. </documentation>
		</annotation>
	</element>
	<element name="PersonSuffixName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Qualifications, awards, and/or honours normally appended to a
				person’s name. <H4>Example</H4>O.M. </documentation>
		</annotation>
	</element>
	<element name="PictureActionHorizontalSafePercentage" type="decimal">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defining the transmission format allows programme makers
				shooting in 16:9 to limit key elements of the scene to the section of the frame that
				will be seen by the viewer. This attribute specifies the percentage of the image
				which will lie outside the vertical safe area for action (cf. graphics). The
				percentage is of the total raster and not merely of the shoot to protect area and
				the figure is for both left and right sides of the screen. Thus, a safe area of 3.5%
				would mean that 7% of the total horizontal portion of the picture is regarded as
				unsafe. <H4>Example</H4>3.5 </documentation>
		</annotation>
	</element>
	<element name="PictureActionVerticalSafePercentage" type="decimal">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defining the analogue transmission format allows programme
				makers shooting in 16:9 to limit key elements of the scene to the section of the
				frame that will be seen by the viewer. This attribute specifies the percentage of
				the image which will lie outside the vertical safe area for action (cf. graphics).
				The percentage is of the total raster and not merely of the shoot to protect area
				and the figure is for both top and bottom of the screen. Thus, a safe area of 3.5%
				would mean that 7% of the total vertical portion of the picture is regarded as
				unsafe. <H4>Example</H4>3.5 </documentation>
		</annotation>
	</element>
	<element name="PictureDisplayFormatCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4> Identifies the visual effect of the illuminated portion of the
				image as displayed within the complete image at the relevant intended display aspect
				ratio. Effects include: Full frame, Letterbox, Pillarbox. <H4>Example</H4>
				<H4>ReferenceData</H4>PictureDisplayFormatCodeCS </documentation>
		</annotation>
	</element>
	<element name="PictureGraphicsHorizontalSafePercentage" type="decimal">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defining the analogue transmission format allows programme
				makers shooting in 16:9 to limit key elements of the scene to the section of the
				frame that will be seen by the viewer. This attribute specifies the percentage of
				the image which will lie outside the vertical safe area for graphics (cf. action).
				The percentage is of the total raster and not merely of the shoot to protect area
				and the figure is for both sides of the screen. Thus, a safe area of 3.5% would mean
				that 7% of the total horizontal portion of the picture is regarded as unsafe.
					<H4>Example</H4>10 </documentation>
		</annotation>
	</element>
	<element name="PictureGraphicsVerticalSafePercentage" type="decimal">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defining the analogue transmission format allows programme
				makers shooting in 16:9 to limit key elements of the scene to the section of the
				frame that will be seen by the viewer. This attribute specifies the percentage of
				the image which will lie outside the vertical safe area for graphics (cf. action).
				The percentage is of the total raster and not merely of the shoot to protect area
				and the figure is for both top and bottom of the screen. Thus, a safe area of 5%
				would mean that 10% of the total vertical portion of the picture is regarded as
				unsafe. <H4>Example</H4>5 </documentation>
		</annotation>
	</element>
	<element name="PictureIntendedDisplayAspectRatio" type="pmeta:AspectRatioType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The aspect ratio of the complete image as intended for display
				(including any black edges) and, thus, the aspect ratio at which the complete image
				must be displayed to avoid distortion. Expressed as a quantitative relation between
				two integers, the values being separated by a colon. <H4>Aliases</H4>Aspect ratio.
					<H4>Example</H4>4:3, 16:9 <H4>ReferenceData</H4>VisualAspectRatioCS
			</documentation>
		</annotation>
	</element>
	<element name="PictureOriginalFramingAspectRatio" type="pmeta:AspectRatioType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The aspect ratio of the shot as it is framed for capture and,
				thus, the aspect ratio at which the image must be displayed to avoid distortion.
				Expressed as a quantitative relation between two integers, the values being
				separated by a colon. <H4>Example</H4>4:3, 16:9
				<H4>ReferenceData</H4>VisualAspectRatioCS </documentation>
		</annotation>
	</element>
	<element name="ProductionEndDate" type="date">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The date on which the production of the material referred to in
				the encompassing set or communication was completed. <H4>Example</H4>2007-03-04
					<H4>ReferenceData</H4>ISO 8601 </documentation>
		</annotation>
	</element>
	<element name="ProductionStartDate" type="date">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The date on which the production of the material started.
					<H4>Example</H4>2007-01-04 <H4>ReferenceData</H4>ISO 8601 </documentation>
		</annotation>
	</element>
	<element name="ProgrammeDescription" type="pmeta:ProgrammeDescriptionType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A complexType to communicate programme descriptive information.
			</documentation>
		</annotation>
	</element>
	<element name="ProgrammeDetails" type="pmeta:ProgrammeDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies and describe a programme . </documentation>
		</annotation>
	</element>
	<element name="ProgrammeEpisodeTitle" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A name given to a particular programme in a group of programmes.
					<H4>Example</H4>A programme in the series ’Panorama’ may also have the Programme
				Title ’Panorama’ and an Episode Title ’The BSE Crisis’; a programme in the series
				’Dr Who and the Aztecs’ may have a Programme Title ’Dr Who’ and an Episode Title
				’The Warrior of Death’. </documentation>
		</annotation>
	</element>
	<element name="ProgrammeFirstEpisodeCount" type="nonNegativeInteger">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>When a programme is made of more than one episode of a series,
				the number of the first one and the last one used in the programme can be
				communicated. <H4>Example</H4>NCIS2006 might have 50 episodes. A broadcaster may
				have defined as a programme the sequence of episodes 22,23,24, in which case the
				first is 22. </documentation>
		</annotation>
	</element>
	<element name="ProgrammeGroup" type="pmeta:ProgrammeGroupType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies and describes a group of programmes. </documentation>
		</annotation>
	</element>
	<element name="ProgrammeGroupDescription" type="pmeta:ProgrammeGroupDescriptionType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4> Provides information necessary to describe a programme group.
			</documentation>
		</annotation>
	</element>
	<element name="ProgrammeGroupEpisodeQuantity" type="nonNegativeInteger">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The number of programme episodes in serial or series.
					<H4>Example</H4>20 </documentation>
		</annotation>
	</element>
	<element name="ProgrammeGroupIdentification" type="pmeta:ProgrammeGroupIdentificationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides information necessary to identify a programme group.
			</documentation>
		</annotation>
	</element>
	<element name="ProgrammeGroupOrderedFlag" type="boolean">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>This indicates whether within the programme group the individual
				programmes or episodes have to be ordered for giving the correct meaning to the
				programme group. <H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ProgrammeGroupSubtitle" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A secondary title attributed to the programme group.
					<H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ProgrammeGroupSynopsis" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A synopsis of a group of programmes. <H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ProgrammeGroupTitle" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A commercial title attributed to the programme group.
					<H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ProgrammeGroupTitleHistory" type="pmeta:ProgrammeGroupTitleHistoryType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides information on the different titles used for the
				programme group during its lifetime. </documentation>
		</annotation>
	</element>
	<element name="ProgrammeGroupTransmissionCycle" type="pmeta:ProgrammeGroupTransmissionCycleType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Indicates the cycle of transmission of the programmes of the
				group by its code or name. <H4>Example</H4>Weekly, monthly <H4>ReferenceData</H4>.
			</documentation>
		</annotation>
	</element>
	<element name="ProgrammeGroupTransmissionCycleCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Indicates the cycle of transmission of the programmes of the
				group. <H4>Example</H4>Weekly, monthly
				<H4>ReferenceData</H4>TransmissionGroupCycleCodeCS </documentation>
		</annotation>
	</element>
	<element name="ProgrammeGroupTransmissionCycleName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Indicates the cycle of transmission of the programmes of the
				group. <H4>Example</H4>weekly, monthly <H4>ReferenceData</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ProgrammeGroupWorkingTitle" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A temporary title attributed to the programme group.
					<H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ProgrammeIdentification" type="pmeta:ProgrammeIdentificationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides minimum information necessary to identify a programme.
			</documentation>
		</annotation>
	</element>
	<element name="ProgrammeInformation" type="pmeta:ProgrammeInformationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides detailed descriptive information about a programme.
			</documentation>
		</annotation>
	</element>
	<element name="ProgrammeLastEpisodeCount" type="nonNegativeInteger">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>When a programme is made of more than one episode of a series,
				the number of the first one and the last one used in the programme can be
				communicated. <H4>Example</H4>NCIS2006 might have 50 episodes. A broadcaster may
				have defined as a programme the sequence of episodes 22,23,24, in which case the
				last is 24. </documentation>
		</annotation>
	</element>
	<element name="ProgrammeScript" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A text script for a complete programme. This may be a complete
				production script or it may only contain the spoken text. It differs, therefore,
				from a media object script which is a description of a single media object in terms
				applicable to that media object type. <H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ProgrammeSubtitle" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The secondary title given to a programme by its producer
					<H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ProgrammeSynopsis" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The synopsis, short description, of a programme <H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ProgrammeTitle" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The title given to a programme by its producer .
					<H4>Example</H4>Tony Bennet: Live at Leeds Castle. </documentation>
		</annotation>
	</element>
	<element name="ProgrammeTitleHistory" type="pmeta:ProgrammeTitleHistoryType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Recapitulates the history of titles attributed to a programme
				during its lifetime. </documentation>
		</annotation>
	</element>
	<element name="ProgrammeWorkingTitle" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The title given to a programme by its producer before the final
				title is decided. This may be used for reasons of commercial security and resources
				may be booked to the working title as a way of assuring their availability.
					<H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ProgrammeEpisodeCount" type="nonNegativeInteger">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Indicates the number of an episode in a series. </documentation>
		</annotation>
	</element>
	<element name="PublicationEventCount" type="nonNegativeInteger">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>This is a number of complete publications of a specified
				programme group, programme, item, or media object. The exact meaning of the
				attribute is clarified and circumscribed by the set within which it is used.
					<H4>Example</H4>12 </documentation>
		</annotation>
	</element>
	<element name="PublicationEventEnd" type="pmeta:TimestampType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Signals the date at which the publication stops.
			</documentation>
		</annotation>
	</element>
	<element name="PublicationEventIntention" type="pmeta:ClassificationTermType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Describes through a code and / or name the intention of the
				publisher when releasing the content. <H4>Example</H4>
				<H4>ReferenceData</H4>ebu:IntentionCodeCS, tva:IntentionCS </documentation>
		</annotation>
	</element>
	<element name="PublicationEventLiveTransmissionFlag" type="boolean">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4> A flag communicating whether a publication event is a live
				transmission or not. </documentation>
		</annotation>
	</element>
	<element name="PublicationEventStart" type="pmeta:TimestampType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Informs when the material will be published . </documentation>
		</annotation>
	</element>
	<element name="PublicationEventTransmissionService" type="pmeta:ServiceType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies the service on which a publication event took place.
			</documentation>
		</annotation>
	</element>
	<element name="ReferenceAttribute" type="pmeta:ClassificationTermType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a term used for external reference classification by
				its code or name. <H4>Example</H4>
				<H4>ReferenceData</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ReferenceAttributeValueName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Contains the value of an attribute (as opposed to the attribute
				code or name) from an external (non-P/Meta) description scheme. <H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ReferenceInformation" type="pmeta:ReferenceInformationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides additional reference information for external
				classification. </documentation>
		</annotation>
	</element>
	<element name="ReferenceScheme" type="pmeta:ClassificationSchemeIdentificationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a classification scheme used for external reference
				classification, by its code and/ or name. </documentation>
		</annotation>
	</element>
	<element name="ReferenceYear" type="gYear">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The year with which a particular production is generally
				associated for cataloguing and reference purposes. This may either refer to the year
				of production or of first publication (or of first publication in a particular
				version) but is generally synonymous with the year given in the copyright or
				performance copyright. <H4>Example</H4>1989, 2007 <H4>Note</H4>Although some
				instances of year of reference may be shown in Roman Numerals within copyright
				statements (e.g. MCMLXXXVII for 1987) that P/Meta will hold the value in Arabic
				Numerals. </documentation>
		</annotation>
	</element>
	<element name="RelatedProgrammeInformation" type="pmeta:RelatedProgrammeInformationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides information on related programme and their relations to
				the programme or programme item under consideration. <H4>Aliases</H4>Similar
				programme information </documentation>
		</annotation>
	</element>
	<element name="ReviewCompleteText" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The complete text of a review. </documentation>
		</annotation>
	</element>
	<element name="ReviewContribution" type="pmeta:ContributionType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides information on the contributors to the review, e.g. the
				author </documentation>
		</annotation>
	</element>
	<element name="ReviewDetails" type="pmeta:ReviewDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides the reviews and related publication information
			</documentation>
		</annotation>
	</element>
	<element name="ReviewExcerptText" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A text selected and extracted from a review. </documentation>
		</annotation>
	</element>
	<element name="ReviewIdentifier" type="pmeta:IdentifierDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides a identifier for a review. </documentation>
		</annotation>
	</element>
	<element name="ReviewSubtitle" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The secondary title given to a review by its author.
			</documentation>
		</annotation>
	</element>
	<element name="ReviewTitle" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The title given to a review by its author. </documentation>
		</annotation>
	</element>
	<element name="RightConditionDescription" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A description of the conditions which govern or restrict the
				Right. <H4>Aliases</H4>Usage restriction <H4>Example</H4>a specific media asset
				cannot be published over the Christmas period. </documentation>
		</annotation>
	</element>
	<element name="RightEndDate" type="date">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The end date of the period covered by this Right, i. e. the date
				to which the rights can be exploited. <H4>Aliases</H4>Expiration
				<H4>Example</H4>2007-06-21 <H4>ReferenceData</H4>ISO 8601 </documentation>
		</annotation>
	</element>
	<element name="RightExclusivityFlag" type="boolean">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4> Indicates whether the right is exclusively granted to the
				licensee. </documentation>
		</annotation>
	</element>
	<element name="RightGrantOfRightsMedia" type="pmeta:RightGrantOfRightsMediaType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies the media covered by the rights. </documentation>
		</annotation>
	</element>
	<element name="RightGrantOfRightsMediaCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A controlled code defining the type of media covered by the
				rights granted by a contract/ agreement. <H4>Aliases</H4>Media Code <H4>Example</H4>
				<H4>ReferenceData</H4>RightGrantOfRightsMediaCodeCS </documentation>
		</annotation>
	</element>
	<element name="RightGrantOfRightsMediaName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A textual description of the type of media covered by the rights
				granted by a contract/ agreement. <H4>Aliases</H4>Media Name <H4>Example</H4>Basic
				Cable Service; Pay Satellite Service; Theatric Distribution. </documentation>
		</annotation>
	</element>
	<element name="RightsManagementAuthority" type="pmeta:OrganisationDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies the authority managing the rights. </documentation>
		</annotation>
	</element>
	<element name="RightStartDate" type="date">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The start date of the period covered by this Right. i. e. the
				date from which the rights can be exploited. <H4>Example</H4>2006-05-07
					<H4>ReferenceData</H4>ISO 8601 </documentation>
		</annotation>
	</element>
	<element name="RightSublicenceFlag" type="boolean">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Indicates whether the granted right can be granted by the
				licensee in sub-licence to third parties. </documentation>
		</annotation>
	</element>
	<element name="RightTerritoryDescription" type="pmeta:RightTerritoryDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Description of the geographical territory over which the
				assigned right(s) apply. </documentation>
		</annotation>
	</element>
	<element name="RightTerritoryDetails" type="pmeta:RightTerritoryDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A description of the geographical territory over which the
				assigned right(s) apply. </documentation>
		</annotation>
	</element>
	<element name="RightTransmissionCount" type="nonNegativeInteger">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The right to transmit specified material a set number of times
				(specified by the attribute value) as granted in an agreement or contract.
					<H4>Example</H4>1, 10 </documentation>
		</annotation>
	</element>
	<element name="RightType" type="pmeta:RightTypeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides the type of right by a code and a description.
			</documentation>
		</annotation>
	</element>
	<element name="RightTypeCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The internationally agreed identifier for the type of right.
					<H4>Example</H4>
				<H4>ReferenceData</H4>RightTypeCodeCS </documentation>
		</annotation>
	</element>
	<element name="RightTypeDescription" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Describes the type of right, for example the right to broadcast
				or the right to publish. <H4>Example</H4>’hereby grants to the Licensee a
				non-exclusive licence to incorporate the Selected Material in the Licensee’s
				production (’the Production’) and to exploit the Production as it incorporates the
				Selected Material on the following conditions: ’ </documentation>
		</annotation>
	</element>
	<element name="RoleDetailsLanguage" type="pmeta:LanguageType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides the language in which the role details are expressed.
			</documentation>
		</annotation>
	</element>
	<element name="RoleName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The name given, where appropriate, to the role played by a
				contributor in a production. <H4>Example</H4>Lady Bracknell; First Policeman;
				Ulysses; Mother Courage. </documentation>
		</annotation>
	</element>
	<element name="RoleType" type="pmeta:RoleTypeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defines a type of role involved in a contribution from a person
				or organisation. </documentation>
		</annotation>
	</element>
	<element name="RoleTypeCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The title of the type of role, responsibility, or task
				undertaken by an organisation or an individual in the development, management or
				control of material. <H4>Example</H4>
				<H4>ReferenceData</H4>ebu:RoleCodeCS (superseding ebu:EscortRoleCS),
			</documentation>
		</annotation>
	</element>
	<element name="RoleTypeName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The title of the type of role, responsibility, or task
				undertaken by an organisation or an individual in the development, management or
				control of material. <H4>Aliases</H4>Staff, crew <H4>Example</H4>Cameraman;
				Producer; Chaperone; Animal Trainer; Publicist </documentation>
		</annotation>
	</element>
	<element name="Series" type="pmeta:SeriesType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies and describes a series. </documentation>
		</annotation>
	</element>
	<element name="ServiceHandoffTime" type="unsignedLong">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>
				<H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ServiceLineupTime" type="unsignedLong">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>
				<H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ServiceName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides the name of a service. <H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="ServiceTransmissionBitrate" type="nonNegativeInteger">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Specifies the bitrate (in bits per second) allocated to a
				transmission service, used for instance for a material exchange. <H4>Aliases</H4>Bit
				rate <H4>Example</H4>24000 </documentation>
		</annotation>
	</element>
	<element name="ShiGoodShotIndicator" type="boolean">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A flag, held against the original instance of any shot media
				object, indicating that the creator suggests this particular take to be that
				preferred for the purposes for which it was created. </documentation>
		</annotation>
	</element>
	<element name="ShotLogoIndicator" type="boolean">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A flag which indicates that all instances of the specified shot
				contain an on-screen logo. This only applies to shot media objects. </documentation>
		</annotation>
	</element>
	<element name="SignLanguage" type="pmeta:SignLanguageType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies and describes a sign languages. </documentation>
		</annotation>
	</element>
	<element name="SignLanguageCode" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A codified value for a system of sign language.
				<H4>Example</H4>ASE; BHO; GSG; ISE <H4>ReferenceData</H4>ebu:SignLanguageCodeCS
			</documentation>
		</annotation>
	</element>
	<element name="SignLanguageDetails" type="pmeta:SignLanguageDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides information on the sign languages associated with the
				material. </documentation>
		</annotation>
	</element>
	<element name="SignLanguageName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The name of a system of sign language. <H4>Example</H4>British
				Sign Language, American Sign Language. </documentation>
		</annotation>
	</element>
	<element name="SignLanguagePrimaryLanguageIndicator" type="boolean">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Indicates whether sign language is the primary language of the
				content, or whether it has been added later. </documentation>
		</annotation>
	</element>
	<element name="SignLanguageTranslationIndicator" type="boolean">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Indicates whether the sign language used is a direct translation
				of the spoken dialogue of the content or is a commentary or summary of what is going
				on. </documentation>
		</annotation>
	</element>
	<element name="SIUnitOfMeasure" type="pmeta:SIUnitOfMeasureType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Describes a unit. </documentation>
		</annotation>
	</element>
	<element name="SIUnitOfMeasureCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The symbol for a particular standard unit for the measurement of
				something (e.g. volume, size, weight, power, resistance, etc.). <H4>Example</H4>km;
				W; A; m; s; K; <H4>ReferenceData</H4>SI/BIPM (Bureau International de Poids et
				mesures) </documentation>
		</annotation>
	</element>
	<element name="SIUnitOfMeasureName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The name of a particular standard unit for the measurement of
				something (e.g. volume, size, weight, power, resistance, etc.) <H4>Aliases</H4>
				<H4>Example</H4>meter, ampere <H4>ReferenceData</H4>SI/BIPM (Bureau International de
				Poids et mesures) </documentation>
		</annotation>
	</element>
	<element name="SiUnitOfMeasureQuantity" type="float">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A numeric value representing a particular amount of units
				specified by the accompanying unit of measure attribute. <H4>Example</H4>10.42
			</documentation>
		</annotation>
	</element>
	<element name="StorageIdentifier" type="pmeta:IdentifierDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>An identity used by an organisation for a storage medium
				occurrence, e.g. a tape number <H4>ReferenceData</H4>StorageIdentifîerTypeCodeCS
				shall be used in IdentifierDetails </documentation>
		</annotation>
	</element>
	<element name="StorageInstance" type="pmeta:StorageInstanceType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defines how an instance of content is being stored.
			</documentation>
		</annotation>
	</element>
	<element name="StorageType" type="pmeta:StorageTypeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defines a type of storage </documentation>
		</annotation>
	</element>
	<element name="StorageTypeCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The code given to identify the form of storage used for
				material. "Form of storage" is a bundled definition which includes all of the
				elements of specification required to support storage. The attribute value table
				points to the specification(s) defining everything needed to extract the information
				from the medium: the physical container (e.g. D3 cassette), the medium (e.g. tape),
				any significant characteristic of the medium (e.g. metal-particle tape),
				characteristics of the head-to-tape interface (e.g. head penetration), and track
				format. <H4>Example</H4>
				<H4>ReferenceData</H4>ebu:StorageMediaTypeCode </documentation>
		</annotation>
	</element>
	<element name="StorageTypeName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The name given to a form of storage used for material. "Form of
				storage" is a bundled definition which includes all of the elements of specification
				required to support storage. The attribute value table points to the
				specification(s) defining everything needed to extract the information from the
				medium: the physical container (e.g. D3 cassette), the medium (e.g. tape), any
				significant characteristic of the medium (e.g. metal-particle tape), characteristics
				of the head-to-tape interface (e.g. head penetration), and track format.
					<H4>Example</H4>DAT; MiniDisc; U-MATIC SP; Digital D3 </documentation>
		</annotation>
	</element>
	<element name="SubtitleFlag" type="boolean">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>indicates whether an item/programme/programme group includes
				subtitles. The nature of the subtitle (e.g. closed caption or teletext) is
				communicated as information related to the subtitle text object. </documentation>
		</annotation>
	</element>
	<element name="TechnicalDetails" type="pmeta:TechnicalDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides technical information. </documentation>
		</annotation>
	</element>
	<element name="TerritoryCodeScheme" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies the classification scheme used to give the code of a
				country, region or subdivision <H4>Example</H4>ISO3166-1 for countries, ISO 3166-2
				for regions and subdivisions, United Nations' Standard Country or Area Codes for
				Statistical Use for countries, regions and subdivisions
				<H4>ReferenceData</H4>ebu:UNTerritoryCodeCS , ebu:Iso3166CountryCodeCS
			</documentation>
		</annotation>
	</element>
	<element name="TerritoryCode" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4> controlled code for a Territory. The code may be selected from
				any of the referenced sources of predefined values. ISO 3166-1 provides country
				codes, ISO 3166-2 country sub-division codes, and EBU/PMeta provides codes only for
				a few particular cases. <H4>Example</H4>GB; IT-MI; DE; XW; XE; XX
					<H4>ReferenceData</H4>ISO 3166-1 if territories correspond to countries or ISO
				3166-2 if territories corresponds to groups of countries or subdivisions such as
				regions. ebu:UNTerritoryCodeCS, ebu:Iso3166CountryCodeCS </documentation>
		</annotation>
	</element>
	<element name="TerritoryName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides the name of a country, region, etc.
				<H4>Example</H4>France, Brittany <H4>ReferenceData</H4>ISO 3166 </documentation>
		</annotation>
	</element>
	<element name="TextUsageType" type="pmeta:TextUsageTypeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides indication of the usage made of text material.
			</documentation>
		</annotation>
	</element>
	<element name="TextUsageTypeCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>An element to define the type of usage for which a text data
				object is intended. For example - Sub-titles; WEB Text. <H4>Example</H4>
				<H4>ReferenceData</H4>TextUsageTypeCodeCS </documentation>
		</annotation>
	</element>
	<element name="TextUsageTypeName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides a type of usage for which a text data object text is
				intended. <H4>Example</H4>Subtitles; WEB Text. </documentation>
		</annotation>
	</element>
	<element name="TitleHistory" type="pmeta:TitleHistoryType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Recapitulates the history of titles attributed to the piece of
				content during its lifetime. </documentation>
		</annotation>
	</element>
	<element name="TotalBroadcastTransmissionsCount" type="nonNegativeInteger">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides the total number of times the material has been
				transmitted. <H4>Example</H4>3 </documentation>
		</annotation>
	</element>
	<element name="TransmissionPublicationDetails" type="pmeta:TransmissionPublicationDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides basic information identifying individual broadcast
				publication events. </documentation>
		</annotation>
	</element>
	<element name="TransmissionPublicationSummary" type="pmeta:TransmissionPublicationSummaryType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides minimum information on the publication of the material.
			</documentation>
		</annotation>
	</element>
	<element name="UnclassifiedSubjectContext" type="pmeta:ExplanatoryNoteType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides contextual additional information concerning an
				unclassified subject. <H4>Example</H4>
			</documentation>
		</annotation>
	</element>
	<element name="UsageReportDueDate" type="date">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides the date by which a contractual report of material
				usage is due. <H4>Example</H4>2007-06-19 <H4>ReferenceData</H4>ISO 8601
			</documentation>
		</annotation>
	</element>
	<element name="ValidFrom" type="pmeta:TimestampType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides a time reference from when a statement or action is
				valid. <H4>Example</H4>
				<H4>ReferenceData</H4>ISO 8601 </documentation>
		</annotation>
	</element>
	<element name="ValidityWindow" type="pmeta:ValidityWindowType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides time reference points within which a statement or
				action is valid. </documentation>
		</annotation>
	</element>
	<element name="ValidTo" type="pmeta:TimestampType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides a time reference until when a statement or action is
				valid. <H4>Example</H4>
				<H4>ReferenceData</H4>ISO 8601 </documentation>
		</annotation>
	</element>
	<element name="VideoActiveLinesPerFrame" type="nonNegativeInteger">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The number of lines within the television frame available to
				carry image information. <H4>Aliases</H4>Vertical resolution <H4>Example</H4>1080
			</documentation>
		</annotation>
	</element>
	<element name="VideoActiveSamplesPerLine" type="nonNegativeInteger">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The number of pixels within the television line available to
				carry image information. <H4>Aliases</H4>Horizontal resolution <H4>Example</H4>1920
			</documentation>
		</annotation>
	</element>
	<element name="VideoAverageBitrate" type="nonNegativeInteger">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides the average bitrate in bits per second at which a
				variable bitrate signal is transmitted <H4>Example</H4>8000000 </documentation>
		</annotation>
	</element>
	<element name="VideoBitsPerPixel" type="nonNegativeInteger">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides the number of bits used to sample the video information
				per pixel. <H4>Aliases</H4>Encoding depth <H4>Example</H4>8 </documentation>
		</annotation>
	</element>
	<element name="VideoCompression" type="pmeta:VideoCompressionType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies by a code or name the compression scheme used to
				encode video </documentation>
		</annotation>
	</element>
	<element name="VideoCompressionCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies by a code the video compression scheme in use
					<H4>Example</H4>
				<H4>ReferenceData</H4>ebu:VideoCompressionCodeCS </documentation>
		</annotation>
	</element>
	<element name="VideoCompressionName" type="string">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies by its name the video compression scheme in use.
					<H4>Aliases</H4>Codec <H4>Example</H4>MPEG H264, VC1, RealPlayer, etc.
					<H4>ReferenceData</H4>VideoCodecCS </documentation>
		</annotation>
	</element>
	<element name="VideoFixedBitrateIndicator" type="boolean">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides an indicator whether the bitrate is fixed in opposition
				to variable. </documentation>
		</annotation>
	</element>
	<element name="VideoFrameRate" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>The rate (in Hz) at which the material should be shown in order
				to achieve the intended editorial effect - expressed in frames per second. This does
				not apply to single frame media objects, for example, stills. <H4>Enumerated
					List</H4>60, 60/1.001, 50, 30, 30/1.001, 25, 24, 24/1.001, 15, 12, 10, 7, 5, 2,
				1, other. ebu:VideoFrameRateCS </documentation>
		</annotation>
	</element>
	<element name="VideoSampleRate" type="nonNegativeInteger">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Specifies the rate of luminance sampling in samples per second.
					<H4>Example</H4>
				<H4>ReferenceData</H4>
			</documentation>
		</annotation>
	</element>
	<element name="VideoSamplingHierarchyCode" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>A code that specifies the component sampling hierarchy for the
				video pixel matrix (SMPTE Data Dictionary). <H4>Aliases</H4>chroma_format
					<H4>Example</H4>..#4:2:2 <H4>ReferenceData</H4>SMPTE RP210,
				ebu:VideSamplingHierarchyCodeCS </documentation>
		</annotation>
	</element>
	<element name="VideoInterlacedSamplingStructureFlag" type="anyURI">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Indicates if the scanning structure is interlaced in opposition
				to progressive. <H4>Aliases</H4>Scanning structure </documentation>
		</annotation>
	</element>
	<element name="VideoTotalLinesPerFrame" type="nonNegativeInteger">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Specifies the number of lines in a total frame in the video
				scanning system. (SMPTE Data Dictionary). <H4>Example</H4>625 </documentation>
		</annotation>
	</element>
	<element name="VideoTotalSamplesPerLine" type="nonNegativeInteger">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Specifies the number of samples in a total line in the video
				pixel matrix <H4>Example</H4>
				<H4>ReferenceData</H4>SMPTE RP 210 </documentation>
		</annotation>
	</element>
	<!-- **************************************************************************************** -->
	<!-- ***********************COMPLEX TYPES********************************************* -->
	<!-- ****************************************************************************************** -->
	<complexType name="AddressType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides address details for a person, organisation or location
			</documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:AddressLineName" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:AddressTownCityName" minOccurs="0"/>
			<element ref="pmeta:AddressCountyStateName" minOccurs="0"/>
			<element ref="pmeta:AddressDeliveryCode" minOccurs="0"/>
			<element ref="pmeta:Country" minOccurs="0"/>
			<element ref="pmeta:AddressTelephoneNumber" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:AddressFacsimileNumber" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:AddressElectronicName" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:AddressWebAddress" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	<complexType name="AudienceScoreRecordingTechniqueType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>identifies a technique to measure audience score by its name or
				code. </documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:AudienceScoreRecordingTechniqueCode"/>
			<element ref="pmeta:AudienceScoreRecordingTechniqueName"/>
		</choice>
	</complexType>
	<complexType name="AudioCompressionType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies an audio encoding scheme by its code and/or name
			</documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:AudioCompressionCode"/>
			<element ref="pmeta:AudioCompressionName"/>
		</choice>
	</complexType>
	<complexType name="AudioFormatType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies the audio format by a code and/or its name.
			</documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:AudioFormatCode"/>
			<element ref="pmeta:AudioFormatName"/>
		</choice>
	</complexType>
	<complexType name="AwardType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies and award and the corresponding ceremony / festival
			</documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:AwardName" minOccurs="0"/>
			<element ref="pmeta:FestivalName" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="BankAccountDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Communicate the bank account details for at least one person or
				organisation, for which more than one account (identified by a name and number as
				well as the bank or other financial institution holding this account) can be
				described. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:AccountHolder"/>
			<element name="BankAccount" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Details for a particular bank account. More than one
						bank account can be provided in one or more bank. </documentation>
				</annotation>
				<complexType>
					<sequence>
						<choice maxOccurs="2">
							<element ref="pmeta:AccountNumber" minOccurs="0"/>
							<element ref="pmeta:AccountName" minOccurs="0"/>
						</choice>
						<element ref="pmeta:BankDetails"/>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="BrandTitleHistoryType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Communicates the title history of a brand of material. Different
				languages can be used. Contextual additional information can be provided in an
				explanatory note. </documentation>
		</annotation>
		<sequence>
			<element name="OriginalBrandTitle" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Concerns the original brand title. </documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:Language" minOccurs="0"/>
						<element ref="pmeta:BrandTitle"/>
					</sequence>
				</complexType>
			</element>
			<element name="OtherBrandTitle" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Concerns any other title. </documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:Language" minOccurs="0"/>
						<element ref="pmeta:Country" minOccurs="0"/>
						<element ref="pmeta:BrandTitle"/>
						<element ref="pmeta:ExplanatoryNote" minOccurs="0"/>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="ClassificationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defines a list of classified terms in a multi-dimensional
				hierarchical way possibly in different languages. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element name="ClassificationIndex" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Provides one or more list of classified terms from one
						or more classification schemes. </documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:ClassificationSchemeIdentification" minOccurs="0"/>
						<element ref="pmeta:ClassificationData" maxOccurs="unbounded"/>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="ClassificationDimensionType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Characterises a classification dimension by its code and/or
				name. <H4>Aliases</H4>Sub-category </documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:ClassificationDimensionCode"/>
			<element ref="pmeta:ClassificationDimensionName"/>
		</choice>
	</complexType>
	<complexType name="ClassificationSchemeIdentificationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Characterises a classification scheme by its code and/or name.
			</documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:ClassificationSchemeCode"/>
			<element ref="pmeta:ClassificationSchemeName"/>
		</choice>
	</complexType>
	<complexType name="ClassificationTermType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defines a classification term by its dimension, and term's code
				and/or name </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:ClassificationDimension" minOccurs="0"/>
			<element ref="pmeta:ClassificationTerm"/>
		</sequence>
	</complexType>
	<complexType name="ClassificationTermTypeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a classification term by its code and/or name
			</documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:ClassificationTermCode" maxOccurs="unbounded"/>
			<element ref="pmeta:ClassificationTermName"/>
		</choice>
	</complexType>
	<complexType name="ContractClausesAndRightsListType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides information about contract clauses and granted rights
				which are specific to the segments of content delimited in time intervals.
			</documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element name="SegmentDetails" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Provides information on a segment identified by time
						boundaries. </documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:SegmentStart" minOccurs="0"/>
						<element ref="pmeta:SegmentEnd" minOccurs="0"/>
						<element name="MediaObjectBasicIdentification" minOccurs="0"
							maxOccurs="unbounded">
							<annotation>
								<documentation xml:lang="EN">
									<H4>Description</H4>Provides information on media object
									composing a segment. </documentation>
							</annotation>
							<complexType>
								<sequence>
									<element ref="pmeta:MediaObjectTitle" minOccurs="0"/>
									<element ref="pmeta:Identifier" minOccurs="0"/>
									<element ref="pmeta:MediaObjectTypeCode" minOccurs="0"/>
								</sequence>
							</complexType>
						</element>
						<element name="Rights">
							<annotation>
								<documentation xml:lang="EN">
									<H4>Description</H4>Provides information on rights related to a
									particular segment. </documentation>
							</annotation>
							<complexType>
								<choice maxOccurs="3">
									<element ref="pmeta:ContractNumber"/>
									<element ref="pmeta:ContractClauseDescription" minOccurs="0"
										maxOccurs="unbounded"/>
									<element ref="pmeta:GrantOfRightsDetails" minOccurs="0"
										maxOccurs="unbounded"/>
								</choice>
							</complexType>
						</element>
						<element ref="pmeta:ContractingParties" minOccurs="0"/>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="ContractDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Communicates all the details of a contract/agreement between two
				parties for the use/exploitation of one, or more, media assets. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:ContractType" minOccurs="0"/>
			<element ref="pmeta:ContractNumber" minOccurs="0"/>
			<element ref="pmeta:ContractDate" minOccurs="0"/>
			<element ref="pmeta:Licensor" minOccurs="0"/>
			<element ref="pmeta:ContractualMaterial" minOccurs="0"/>
			<element ref="pmeta:Licensee" minOccurs="0"/>
			<element ref="pmeta:LicenseCoveredMaterial" minOccurs="0"/>
			<element ref="pmeta:ContractFeesDetails" minOccurs="0"/>
			<element ref="pmeta:ContractTermsOfBusinessDescription" minOccurs="0"/>
			<element ref="pmeta:ContractClauseDescription" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:GrantOfRightsDetails" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:UsageReportDueDate" minOccurs="0"/>
			<element ref="pmeta:LicensorSignatory" minOccurs="0"/>
			<element ref="pmeta:LicenseeSignatory" minOccurs="0"/>
			<element ref="pmeta:TechnicalDetails" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="ContractFeesDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Detail contract fees and their terms of payment including the
				details of the account into which the fees should be paid. If the fee is payable by
				instalments, these can also be communicated. The payment due date is defined as well
				as the bank (or, where relevant, branch) holding the account </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:ContractNumber" minOccurs="0"/>
			<element ref="pmeta:BankAccountDetails" minOccurs="0" maxOccurs="unbounded"/>
			<element name="FeeDetails" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Provides detailed information on fees per contract line.
					</documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:ContractLine" maxOccurs="2"/>
						<element ref="pmeta:ContractLineAmount" minOccurs="0"/>
					</sequence>
				</complexType>
			</element>
			<element name="Instalment" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Provides information on payment instalment.
					</documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:ContractPaymentInstalmentCount" minOccurs="0"/>
						<element ref="pmeta:ContractPaymentInstalmentDueDate" minOccurs="0"/>
						<element ref="pmeta:ContractPaymentInstalmentAmount" minOccurs="0"/>
					</sequence>
				</complexType>
			</element>
			<element ref="pmeta:ContractTotalCost" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="ContractLineType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Describes a contract line by a contract line number and/or by
				the line itself. </documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:ContractLineNumber" minOccurs="0"/>
			<element ref="pmeta:ContractLineName" minOccurs="0"/>
		</choice>
	</complexType>
	<complexType name="ContractTypeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defines a type of contract through a code and/or name.
			</documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:ContractTypeCode"/>
			<element ref="pmeta:ContractTypeName"/>
		</choice>
	</complexType>
	<complexType name="ContributionType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Communicates various details about a contribution, or
				contributions, made to something by one or more persons or organisations.
			</documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element name="RoleDetails" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Provides detailed information on the role played by the
						contributor. </documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:RoleDetailsLanguage" minOccurs="0"/>
						<element ref="pmeta:RoleType"/>
						<element name="ContributionDetails" minOccurs="0" maxOccurs="unbounded">
							<annotation>
								<documentation xml:lang="EN">
									<H4>Description</H4>Provides a name for the role as well as
									detailed information regarding the contributor (a person or
									organisation). </documentation>
							</annotation>
							<complexType>
								<sequence>
									<element ref="pmeta:RoleName" minOccurs="0"/>
									<element ref="pmeta:ContributorDetails" minOccurs="0"
										maxOccurs="unbounded"/>
								</sequence>
							</complexType>
						</element>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="ContributorDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides detailed information on e contributor (a person or
				organisation). An explanatory note allows providing additional contextual
				information. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<choice maxOccurs="2">
				<element ref="pmeta:PersonDetails" minOccurs="0"/>
				<element ref="pmeta:OrganisationDetails" minOccurs="0"/>
			</choice>
			<element ref="pmeta:ExplanatoryNote" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="CountryType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a country by its code and/or name </documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:CountryCode"/>
			<element ref="pmeta:CountryName"/>
		</choice>
	</complexType>
	<complexType name="CurrencyAmountDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Communicate in the original or a foreign name the currency and
				the amount of the transaction. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:Currency"/>
			<element ref="pmeta:CurrencyAmount"/>
		</sequence>
	</complexType>
	<complexType name="CurrencyType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a currency by a code and/or a name. </documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:CurrencyCode"/>
			<element ref="pmeta:CurrencyName"/>
		</choice>
	</complexType>
	<complexType name="DataCompressionType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies by its code or name the scheme used to compress data.
			</documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:DataCompressionCode"/>
			<element ref="pmeta:DataCompressionName"/>
		</choice>
	</complexType>
	<complexType name="DeviceTypeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a type of device by its code and/ or name.
			</documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:DeviceTypeCode"/>
			<element ref="pmeta:DeviceTypeName"/>
		</choice>
	</complexType>
	<complexType name="EditorialControlType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defines by its code and/or name a type of editorial control.
			</documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:EditorialControlCode"/>
			<element ref="pmeta:EditorialControlName"/>
		</choice>
	</complexType>
	<complexType name="EditorialControlDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Communicates the editorial control of the programme content at
				the production stage, together with the list of organisation names involved in the
				production, and the year of reference for the production. The order of the
				organisations within the list (where more than one) is expected to have been agreed
				by the organisations themselves and to reflect the importance of each organisation
				in the production, the first one being the most important. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:EditorialControl"/>
			<element name="EditorialControlBy" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Identifies the organisation detaining editorial control,
						and their country of origin. </documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:OrganisationName"/>
						<element ref="pmeta:Country" minOccurs="0"/>
					</sequence>
				</complexType>
			</element>
			<element ref="pmeta:ReferenceYear" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="EquipmentDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Communicates basic details about a piece of equipment used in
				the production, publication, storage, and/or exchange of material. The Contribution
				element is to communicate for instance the manufacturer of the device or any other
				role type related to the equipment for example, operator, supplier keeper, etc .
			</documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:DeviceType" minOccurs="0"/>
			<element ref="pmeta:DeviceModelName" minOccurs="0"/>
			<element ref="pmeta:DeviceNumber" minOccurs="0"/>
			<element ref="pmeta:DeviceSerialNumber" minOccurs="0"/>
			<element ref="pmeta:Contribution" minOccurs="0"/>
		</sequence>
	</complexType>
	<simpleType name="ExplanatoryNoteType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides a uniform text format for contextual additional
				information. </documentation>
		</annotation>
		<restriction base="string"/>
	</simpleType>
	<complexType name="ExternalSchemeExchangeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Communicates information using an attribute, or attributes, from
				a non-P/Meta data description scheme. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:DefaultReferenceScheme"/>
			<element ref="pmeta:ReferenceInformation" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	<complexType name="FileFormatTypeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a type of format file by its code and/or name.
			</documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:FileFormatTypeCode"/>
			<element ref="pmeta:FileFormatTypeName"/>
		</choice>
	</complexType>
	<complexType name="FileInstanceType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Describes the main characteristics of a file instance (format,
				type, Size) and the path to access it. This could be a digital media file (for
				example containing audio, video or still images), or a file which only contains data
				or metadata. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:FileFormatType" minOccurs="0"/>
			<choice>
				<element ref="pmeta:AudioCompression"/>
				<element ref="pmeta:DataCompression"/>
				<element ref="pmeta:VideoCompression"/>
			</choice>
			<element ref="pmeta:FileSize" minOccurs="0"/>
			<element ref="pmeta:FilePathName"/>
		</sequence>
	</complexType>
	<complexType name="GrantOfRightsConditionsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Describe the conditions under which rights are being granted for
				a certain period of time or territory. RightTransmissionCount is optional and it
				absence is equivalent to "unlimited". </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:RightGrantOfRightsMedia"/>
			<element ref="pmeta:RightTransmissionCount" minOccurs="0"/>
			<element ref="pmeta:RightStartDate"/>
			<element ref="pmeta:RightEndDate" minOccurs="0"/>
			<element ref="pmeta:RightSublicenceFlag" minOccurs="0"/>
			<element ref="pmeta:RightExclusivityFlag" minOccurs="0"/>
			<element ref="pmeta:RightTerritoryDetails"/>
			<element ref="pmeta:DubbingSubtitlingGranted" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:RightConditionDescription"/>
		</sequence>
	</complexType>
	<complexType name="GrantOfRightsDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defines the rights granted, and the conditions applying to those
				rights, within the contract/ agreement. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element name="GrantedRights" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Provides detailed information on the rights being
						granted. </documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:RightType"/>
						<element ref="pmeta:RightsManagementAuthority" minOccurs="0"/>
						<element ref="pmeta:GrantOfRightsConditions" maxOccurs="unbounded"/>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="GraphicUsageTypeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a type of graphic usage by its code and/or name.
			</documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:GraphicUsageTypeCode"/>
			<element ref="pmeta:GraphicUsageTypeName"/>
		</choice>
	</complexType>
	<complexType name="IdentifierDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies and qualifies an identifier, which use is defined
				contextual. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:IdentifierType"/>
			<element ref="pmeta:IdentifierNumber"/>
		</sequence>
	</complexType>
	<complexType name="IdentifierTypeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a type of identifier by its code or name.
			</documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:IdentifierTypeName"/>
			<element ref="pmeta:IdentifierTypeCode"/>
		</choice>
	</complexType>
	<complexType name="IdentityMinimumDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Communicates a minimum identifying information for a piece of
				content. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element name="IdentifyingInformation" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Identification is provided through an identifier and
						associate material titles. </documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:Identifier" minOccurs="0"/>
						<element ref="pmeta:TitleHistory" minOccurs="0"/>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="IndexExternalSchemeTermType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies the descriptive scheme where a subject description
				requires the use of an external (i.e. non-EBU) scheme. <H4>Example</H4>Library of
				Congress Subject Headings </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:ClassificationData"/>
			<element ref="pmeta:IndexExternalSchemeTermScope" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="ItemType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies and describes a content item </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:ItemDetails"/>
			<element ref="pmeta:ItemInformation" minOccurs="0"/>
			<element ref="pmeta:ContractDetails" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="ItemDescriptionType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Groups all the information necessary to describe a content item.
			</documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:Duration" minOccurs="0"/>
			<element ref="pmeta:ItemSynopsis" minOccurs="0"/>
			<element ref="pmeta:ItemScript" minOccurs="0"/>
			<element ref="pmeta:ItemStartScriptCueText" minOccurs="0"/>
			<element ref="pmeta:ItemEndScriptCueText" minOccurs="0"/>
			<element ref="pmeta:Keywords" minOccurs="0"/>
			<element ref="pmeta:Location" minOccurs="0"/>
			<element ref="pmeta:OriginationCode" minOccurs="0"/>
			<element ref="pmeta:ColourCode" minOccurs="0"/>
			<element ref="pmeta:SubtitleFlag"/>
			<element ref="pmeta:LanguageHistory" minOccurs="0"/>
			<element ref="pmeta:ReviewDetails" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:SignLanguageDetails" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	<complexType name="ItemDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides detailed descriptive information about a content item.
			</documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:ItemIdentification"/>
			<element ref="pmeta:Classification" minOccurs="0"/>
			<element ref="pmeta:ItemDescription" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="ItemInformationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides additional usage, composition and contractual
				information about a content item. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:TransmissionPublicationDetails" minOccurs="0"/>
			<element ref="pmeta:RelatedProgrammeInformation" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:Duration"/>
			<element ref="pmeta:MediaObjectDetails" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:MaterialExchangeInstance" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:ContractClausesAndRightsList" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="ItemsGroupType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Describes a group of items as a constituent editorial part of an
				individual programme, not necessarily continuous in time, composed of individual
				items (also not necessarily contiguous in time). </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:Identifier" minOccurs="0"/>
			<element ref="pmeta:ExplanatoryNote" minOccurs="0"/>
			<element ref="pmeta:Contribution" minOccurs="0"/>
			<element ref="pmeta:ItemIdentification" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	<complexType name="ItemIdentificationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies and item by its identifier, title and time position
				if only part of larger material, as well as information on who has contributed to
				its creation, etc. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:Identifier"/>
			<element ref="pmeta:ItemSequenceNumber" minOccurs="0"/>
			<element name="ItemStartTime" type="pmeta:TimeMarkType" minOccurs="0"/>
			<element name="ItemEndTime" type="pmeta:TimeMarkType" minOccurs="0"/>
			<element ref="pmeta:ItemTitleHistory"/>
			<element ref="pmeta:ProductionEndDate" minOccurs="0"/>
			<element ref="pmeta:Contribution" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="ItemTitleHistoryType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides the history of titles used to qualify the item during
				its lifetime with contextual information. </documentation>
		</annotation>
		<sequence>
			<element name="OriginalItemTitle" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>List the original titles attributed to the content item
						possibly in different languages. </documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:Language" minOccurs="0"/>
						<element ref="pmeta:ItemTitle"/>
						<element ref="pmeta:ItemSubtitle" minOccurs="0"/>
					</sequence>
				</complexType>
			</element>
			<element name="OtherItemTitle" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>List the titles, other than original, used for this item
						in different countries and in different languages. Reasons can be given
						using the explanatory note. </documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:Language" minOccurs="0"/>
						<element ref="pmeta:Country" minOccurs="0"/>
						<element ref="pmeta:ItemTitle"/>
						<element ref="pmeta:ItemSubtitle" minOccurs="0"/>
						<element ref="pmeta:ExplanatoryNote" minOccurs="0"/>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="KeywordsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides descriptive keywords applied to material, possibly in
				different languages and contexts, for search and query. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element name="Keyword" maxOccurs="unbounded">
				<complexType>
					<sequence>
						<element name="KeywordLanguage" type="pmeta:LanguageType" minOccurs="0"/>
						<choice>
							<sequence>
								<element ref="pmeta:KeywordName" minOccurs="0"/>
								<element name="SubjectContext">
									<annotation>
										<documentation xml:lang="EN">
											<H4>Description</H4>A keyword can be extracted from a
											thesaurus or unclassified. </documentation>
									</annotation>
									<complexType>
										<choice minOccurs="0">
											<element ref="pmeta:UnclassifiedSubjectContext"/>
											<element ref="pmeta:ClassifiedSubjectContext"/>
										</choice>
									</complexType>
								</element>
							</sequence>
							<element ref="pmeta:ClassifiedSubject"/>
						</choice>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="LanguageType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a language by its code or name. </documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:LanguageCode"/>
			<element ref="pmeta:LanguageName"/>
		</choice>
	</complexType>
	<complexType name="LanguageHistoryType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies the original language (or languages) in which the
				material being exchanged was produced and, where different, the language (or
				languages) of the version being exchanged, together with the information about how
				the particular language is used in the context. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element name="OriginalLanguage" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Original language in which content was created.
					</documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:Language"/>
						<element ref="pmeta:LanguageUsage"/>
					</sequence>
				</complexType>
			</element>
			<element name="ExchangedVersionLanguage" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Language in which a version is being used e.g. for
						exchange. </documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:Language"/>
						<element ref="pmeta:LanguageUsage"/>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="LanguageUsageType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Describes the usage of the language <H4>Example</H4>Dubbing,
				subtitling, etc <H4>ReferenceData</H4>LanguageUsageCodeCS </documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:LanguageUsageCode"/>
			<element ref="pmeta:LanguageUsageDescription"/>
		</choice>
	</complexType>
	<complexType name="LocationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides various information about the different sorts of
				location related information known about the material under consideration.
			</documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element name="LocationDetails" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Provides location information on the action, capture and
						settings locations. </documentation>
				</annotation>
				<complexType>
					<sequence>
						<element name="Action" minOccurs="0">
							<complexType>
								<sequence>
									<sequence>
										<element ref="pmeta:LocationActionName" minOccurs="0"/>
										<element ref="pmeta:Address" minOccurs="0"/>
									</sequence>
								</sequence>
							</complexType>
						</element>
						<element name="Capture" minOccurs="0">
							<complexType>
								<sequence>
									<sequence>
										<element ref="pmeta:LocationCaptureName" minOccurs="0"/>
										<element ref="pmeta:Address" minOccurs="0"/>
									</sequence>
								</sequence>
							</complexType>
						</element>
						<element name="Setting" minOccurs="0">
							<complexType>
								<sequence>
									<sequence>
										<element ref="pmeta:LocationSettingName" minOccurs="0"/>
										<element ref="pmeta:Address" minOccurs="0"/>
									</sequence>
								</sequence>
							</complexType>
						</element>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="MaterialType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>
				<H4>Aliases</H4>
				<H4>Example</H4>
				<H4>ReferenceData</H4>
			</documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:ValidityWindow" minOccurs="0"/>
			<element ref="pmeta:ProgrammeGroup" maxOccurs="unbounded"/>
			<element ref="pmeta:ExplanatoryNote" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="MaterialExchangeByServiceInstanceType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Communicates a basic set of information describing a material
				exchange instance when the exchange is performed through a service, such as a
				transmission link, instead of an exchange of a physical medium or a computer file.
			</documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:MaterialExchangeService" minOccurs="0"/>
			<element ref="pmeta:MaterialExchangeStart" minOccurs="0"/>
			<element ref="pmeta:Duration" minOccurs="0"/>
			<element ref="pmeta:ServiceLineupTime" minOccurs="0"/>
			<element ref="pmeta:ServiceHandoffTime" minOccurs="0"/>
			<element ref="pmeta:ServiceTransmissionBitrate" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="MaterialExchangeInstanceType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides identification and descriptive information about an
				instance of material. for exchange, which can be structured in segments and composed
				media objects. More information can be provided on the format (inc. for one or more
				files) and media used for the exchange. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:Identifier" minOccurs="0"/>
			<element name="SegmentDetails" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Describe the segments composing the content instance.
					</documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:SegmentStart" minOccurs="0"/>
						<element ref="pmeta:SegmentEnd" minOccurs="0"/>
						<element ref="pmeta:TechnicalDetails" minOccurs="0"/>
						<element name="ExchangeDetails">
							<annotation>
								<documentation xml:lang="EN">
									<H4>Description</H4>Provides information on the format used for
									exchange. </documentation>
							</annotation>
							<complexType>
								<choice>
									<element ref="pmeta:MaterialExchangeByServiceInstance"
										minOccurs="0"/>
									<element name="StorageDetails" minOccurs="0">
										<annotation>
											<documentation xml:lang="EN">
												<H4>Description</H4>Identifies whether content is
												stored on a removable media (e.g. a tape) or as a
												file (e.g. audio, video, etc.). </documentation>
										</annotation>
										<complexType>
											<sequence>
												<element ref="pmeta:StorageInstance" minOccurs="0"/>
												<element ref="pmeta:FileInstance" minOccurs="0"/>
											</sequence>
										</complexType>
									</element>
								</choice>
							</complexType>
						</element>
						<element name="MediaObjectLocation" minOccurs="0" maxOccurs="unbounded">
							<annotation>
								<documentation xml:lang="EN">
									<H4>Description</H4>Describes the media object composing the
									content instance. <H4>Aliases</H4>Media components, tracks,
									channels, etc. </documentation>
							</annotation>
							<complexType>
								<sequence>
									<element ref="pmeta:MediaObjectTitle" minOccurs="0"/>
									<element ref="pmeta:Identifier" minOccurs="0"/>
									<element ref="pmeta:MediaObjectTypeCode" minOccurs="0"/>
									<element ref="pmeta:MediaObjectTrackIdentifier" minOccurs="0"/>
								</sequence>
							</complexType>
						</element>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="MaterialRelationshipsDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>
				<H4>Aliases</H4>
				<H4>Example</H4>
				<H4>ReferenceData</H4>
			</documentation>
		</annotation>
		<sequence>
			<element name="RelationshipDetails" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Communicates the relationship(s) between a programme and
						or a brand, a programme group, a programme, an item, or a media object.
					</documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:MaterialRelationshipTypeCode"/>
						<element name="RelatedEntity">
							<annotation>
								<documentation xml:lang="EN">
									<H4>Description</H4>Identifies a type of relation by its code
										<H4>Aliases</H4>How related <H4>Example</H4>Is trailer of
										<H4>ReferenceData</H4>HowRelatedCS </documentation>
							</annotation>
							<complexType>
								<choice>
									<element ref="pmeta:BrandTitleHistory"/>
									<element ref="pmeta:ProgrammeGroupIdentification"/>
									<element ref="pmeta:ProgrammeIdentification"/>
									<element ref="pmeta:ItemIdentification"/>
									<element ref="pmeta:MediaObjectIdentification"/>
								</choice>
							</complexType>
						</element>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="MediaObjectType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies and describes a media object. <H4>Aliases</H4>Media
				component, channel, track, etc. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:MediaObjectInformation" minOccurs="0"/>
			<element ref="pmeta:ContractDetails" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="MediaObjectDescriptionType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Communicates an itemised description of a media object. It is
				mandatory to indicate the media object type. Communication of the time interval is
				mandatory, so for media object types which do not have a duration in time, such as a
				photograph, the two values shall both be set to zero. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:MediaObjectTypeCode"/>
			<element name="SegmentDetails" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Describe the segments composing the media object.
					</documentation>
				</annotation>
				<complexType>
					<sequence>
						<element name="MediaObjectStartTime" type="pmeta:TimeMarkType" minOccurs="0"/>
						<element name="MediaObjectEndTime" type="pmeta:TimeMarkType" minOccurs="0"/>
						<element ref="pmeta:MediaObjectScript" minOccurs="0"/>
						<element ref="pmeta:Keywords" minOccurs="0"/>
						<element ref="pmeta:MediaObjectCapturedDescription" minOccurs="0"/>
						<element ref="pmeta:SubtitleFlag"/>
						<element ref="pmeta:Location" minOccurs="0"/>
						<element ref="pmeta:OriginationCode" minOccurs="0"/>
						<element ref="pmeta:ColourCode" minOccurs="0"/>
						<element ref="pmeta:LanguageHistory" minOccurs="0"/>
						<element ref="pmeta:TextUsageType" minOccurs="0"/>
						<element ref="pmeta:GraphicUsageType" minOccurs="0"/>
						<element ref="pmeta:ShotLogoIndicator" minOccurs="0"/>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="MediaObjectDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies and describes a media object. The media object start
				and end points are measured in time elapsed from the beginning of the programme or
				of the item of which the media object is a component.</documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:MediaObjectIdentification"/>
			<element ref="pmeta:MediaObjectDescription" minOccurs="0"/>
			<element ref="pmeta:Classification" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="MediaObjectIdentificationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides minimum information to identify a media object The
				identifier should be suitable for media objects e.g. a UMID. The media object start
				and end points are measured in time elapsed from the beginning of the programme or
				of the item of which the media object is a component. The programme or item is
				identified by a different identification mechanism. This information is not
				necessary in case the media object timeline is the same as that of the programme or
				item in question</documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:Identifier"/>
			<element ref="pmeta:MediaObjectTypeCode"/>
			<element name="MediaObjectStartTime" type="pmeta:TimeMarkType" minOccurs="0"/>
			<element name="MediaObjectEndTime" type="pmeta:TimeMarkType" minOccurs="0"/>
			<element ref="pmeta:MediaObjectTitleHistory" minOccurs="0"/>
			<element ref="pmeta:MaterialCaptureDate" minOccurs="0"/>
			<element ref="pmeta:Contribution" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="MediaObjectInformationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides complementary information regarding the media object.
			</documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:MediaObjectDetails"/>
			<element ref="pmeta:MaterialRelationshipsDetails" minOccurs="0"/>
			<element ref="pmeta:MaterialExchangeInstance" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:ContractClausesAndRightsList" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="MediaObjectTitleHistoryType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides history information on the titles attributed to a media
				object during its lifetime. </documentation>
		</annotation>
		<sequence>
			<element name="OriginalTitle" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>History of the original title possibly in different
						languages </documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:Language" minOccurs="0"/>
						<element ref="pmeta:MediaObjectTitle"/>
					</sequence>
				</complexType>
			</element>
			<element name="OtherTitle" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>History of other than original title possibly in
						different languages for different countries with contextual additional
						information if required. </documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:Language" minOccurs="0"/>
						<element ref="pmeta:Country" minOccurs="0"/>
						<element ref="pmeta:MediaObjectTitle"/>
						<element ref="pmeta:ExplanatoryNote" minOccurs="0"/>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="OrganisationDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Gives basic contact details for an organisation. If more than
				one organisation name, and organisation type (code/name), are used they should be
				ordered to reflect the increasing specificity of the organisation structure. For
				example: BBC:BBC World Service:African Region:Hausa. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:OrganisationName"/>
			<element ref="pmeta:OrganisationType" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:OrganisationIdentifier" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:OrganisationDescription" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:Address" minOccurs="0"/>
			<element ref="pmeta:Contacts" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="OrganisationTypeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a type of organisation by its code or name.
			</documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:OrganisationTypeCode"/>
			<element ref="pmeta:OrganisationTypeName"/>
		</choice>
	</complexType>
	<complexType name="PersonDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Gives basic contact details for a person. Contacts refer a
				primary contact or a public relations agency for an actor/actress, or any personnel
				supporting the person, such as secretaries, make-up artists, hairdressers, and
				personal assistants. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:PersonLastName" minOccurs="0"/>
			<element ref="pmeta:PersonFirstName" minOccurs="0"/>
			<element ref="pmeta:PersonMiddleName" minOccurs="0"/>
			<element ref="pmeta:PersonSalutationShortForm" minOccurs="0"/>
			<element ref="pmeta:PersonSuffixName" minOccurs="0"/>
			<element ref="pmeta:PersonDescription" minOccurs="0"/>
			<element ref="pmeta:PersonIdentifier" minOccurs="0"/>
			<element ref="pmeta:PersonStageName" minOccurs="0"/>
			<element ref="pmeta:Address" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:Contacts" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="ProgrammeDescriptionType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Communicates complete programme descriptive information.
			</documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:Duration" minOccurs="0"/>
			<element ref="pmeta:ProgrammeSynopsis" minOccurs="0"/>
			<element ref="pmeta:ProgrammeScript" minOccurs="0"/>
			<element ref="pmeta:ColourCode" minOccurs="0"/>
			<element ref="pmeta:SubtitleFlag"/>
			<element ref="pmeta:LanguageHistory" minOccurs="0"/>
			<element ref="pmeta:Location" minOccurs="0"/>
			<element ref="pmeta:Keywords" minOccurs="0"/>
			<element ref="pmeta:Award" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:ReviewDetails" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:SignLanguageDetails" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	<complexType name="ProgrammeDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides minimum identification and descriptive information.
			</documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:ProgrammeIdentification"/>
			<element ref="pmeta:Classification" minOccurs="0"/>
			<element ref="pmeta:ProgrammeDescription" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="ProgrammeGroupType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies and describes a group of programmes and the details
				concerning the individual programmes member of this group. </documentation>
		</annotation>
		<sequence>
			<element name="ProgrammeGroupDetails" minOccurs="0">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Provides detailed identification, descriptive an
						relational information concerning a group of programmes. </documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:ProgrammeGroupIdentification"/>
						<element ref="pmeta:Classification" minOccurs="0" maxOccurs="unbounded"/>
						<element ref="pmeta:ProgrammeGroupDescription" minOccurs="0"/>
						<element ref="pmeta:MaterialRelationshipsDetails" minOccurs="0"/>
					</sequence>
				</complexType>
			</element>
			<element name="Programme" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Provides basic information concerning programmes
						belonging to the programme group. </documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:ProgrammeInformation"/>
						<element ref="pmeta:AvailableExchangeFormat" minOccurs="0"
							maxOccurs="unbounded"/>
						<element ref="pmeta:CopyrightHolder" minOccurs="0"/>
						<element ref="pmeta:CopyrightAgent" minOccurs="0"/>
						<element ref="pmeta:ContractDetails" minOccurs="0"/>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="ProgrammeGroupDescriptionType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides detailed descriptive information of a programme group
				as a whole. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:ProgrammeGroupSynopsis" minOccurs="0"/>
			<element ref="pmeta:ColourCode" minOccurs="0"/>
			<element ref="pmeta:SubtitleFlag"/>
			<element ref="pmeta:LanguageHistory" minOccurs="0"/>
			<element ref="pmeta:Location" minOccurs="0"/>
			<element ref="pmeta:Keywords" minOccurs="0"/>
			<element ref="pmeta:Award" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:ReviewDetails" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:SignLanguageDetails" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	<complexType name="ProgrammeGroupIdentificationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides identification information for the programme group as a
				whole. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:Identifier"/>
			<element ref="pmeta:ProgrammeGroupTitleHistory"/>
			<element ref="pmeta:ProgrammeGroupEpisodeQuantity" minOccurs="0"/>
			<element ref="pmeta:ProgrammeGroupOrderedFlag" minOccurs="0"/>
			<element ref="pmeta:Contribution" minOccurs="0"/>
			<element ref="pmeta:ProductionEndDate" minOccurs="0"/>
			<element ref="pmeta:EditorialControlDetails" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="ProgrammeGroupTitleHistoryType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides an history of the titles attributes to a programme
				group during its lifetime. </documentation>
		</annotation>
		<sequence>
			<element name="OriginalProgrammeGroupTitle" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Relates to the original titles of the programme group
						possibly in different languages. </documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:Language" minOccurs="0"/>
						<element ref="pmeta:ProgrammeGroupWorkingTitle" minOccurs="0"/>
						<element ref="pmeta:ProgrammeGroupTitle" minOccurs="0"/>
						<element ref="pmeta:ProgrammeGroupSubtitle" minOccurs="0"/>
					</sequence>
				</complexType>
			</element>
			<element name="OtherProgrammeGroupTitle" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Relates to other than original titles attributed in
						different countries, possibly in different languages. </documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:Language" minOccurs="0"/>
						<element ref="pmeta:Country" minOccurs="0"/>
						<element ref="pmeta:ProgrammeGroupWorkingTitle" minOccurs="0"/>
						<element ref="pmeta:ProgrammeGroupTitle" minOccurs="0"/>
						<element ref="pmeta:ProgrammeGroupSubtitle" minOccurs="0"/>
						<element ref="pmeta:ExplanatoryNote" minOccurs="0"/>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="ProgrammeGroupTransmissionCycleType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies by its code or name a particular cycle of
				transmission for the programme group. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:ProgrammeGroupTransmissionCycleCode" minOccurs="0"/>
			<element ref="pmeta:ProgrammeGroupTransmissionCycleName" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="ProgrammeIdentificationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Communicates information for the identification of an individual
				programme. A programme is identified by an identification number and/or by its
				position e.g. in a series. The programme can be composed of more than one episode
				identified by the count of the first and last episodes </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:Identifier"/>
			<element ref="pmeta:ProgrammeTitleHistory"/>
			<element ref="pmeta:ProgrammeFirstEpisodeCount" minOccurs="0"/>
			<element ref="pmeta:ProgrammeLastEpisodeCount" minOccurs="0"/>
			<element ref="pmeta:Contribution" minOccurs="0"/>
			<element ref="pmeta:ProductionEndDate" minOccurs="0"/>
			<element ref="pmeta:EditorialControlDetails" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="ProgrammeInformationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides detailed information on a programme and on the
				different programme items and/or media object constituting this programme.
			</documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:ProgrammeDetails"/>
			<element ref="pmeta:MaterialRelationshipsDetails" minOccurs="0"/>
			<element ref="pmeta:TransmissionPublicationDetails" minOccurs="0"/>
			<element ref="pmeta:ItemDetails" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:MediaObjectDetails" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:MaterialExchangeInstance" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:ContractClausesAndRightsList" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="ProgrammeTitleHistoryType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides an history of the title attributes to a programme
				during its lifetime. </documentation>
		</annotation>
		<sequence>
			<element name="OriginalProgrammeTitle" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Relates to the original titles possibly in different
						languages. </documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:Language" minOccurs="0"/>
						<element ref="pmeta:ProgrammeWorkingTitle" minOccurs="0"/>
						<element ref="pmeta:ProgrammeTitle" minOccurs="0"/>
						<element ref="pmeta:ProgrammeSubtitle" minOccurs="0"/>
						<element ref="pmeta:ProgrammeEpisodeTitle" minOccurs="0"/>
					</sequence>
				</complexType>
			</element>
			<element name="OtherProgrammeTitle" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Relates to the other than original titles for different
						countries possibly in different languages. </documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:Language" minOccurs="0"/>
						<element ref="pmeta:Country" minOccurs="0"/>
						<element ref="pmeta:ProgrammeWorkingTitle" minOccurs="0"/>
						<element ref="pmeta:ProgrammeTitle" minOccurs="0"/>
						<element ref="pmeta:ProgrammeSubtitle" minOccurs="0"/>
						<element ref="pmeta:ProgrammeEpisodeTitle" minOccurs="0"/>
						<element ref="pmeta:ExplanatoryNote" minOccurs="0"/>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="ReferenceInformationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Describes reference data details from an external reference
				source. It is also possible to attribute a value to the reference term being
				defined. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:Language" minOccurs="0"/>
			<element ref="pmeta:ReferenceScheme" minOccurs="0"/>
			<element ref="pmeta:ReferenceAttribute"/>
			<element ref="pmeta:ReferenceAttributeValueName"/>
		</sequence>
	</complexType>
	<complexType name="RegisteredIdentifiersType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a duly registered identifier with contextual
				information. <H4>Aliases</H4>
				<H4>Example</H4>
				<H4>ReferenceData</H4>
			</documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:Identifier"/>
			<element ref="pmeta:ExplanatoryNote"/>
		</sequence>
	</complexType>
	<complexType name="RelatedProgrammeInformationType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides information on related programme and their relations to
				the programme or programme item under consideration. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:ProgrammeIdentification"/>
			<element ref="pmeta:TransmissionPublicationDetails" minOccurs="0"/>
			<element ref="pmeta:MaterialRelationshipsDetails" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="ReviewDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Communicates the information details about a review. The
				communication of the content of the review is mandatory. This can be achieved either
				by including the complete text, or by at least one (or both) between an extracted
				text and the identification of the review by, for instance, a URI identifier. The
				information about the author of the review is mandatory. It is also mandatory to
				provide the review’s publication information. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:ReviewTitle" minOccurs="0"/>
			<element ref="pmeta:ReviewSubtitle" minOccurs="0"/>
			<element name="ReviewContent">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Provides either the full review or an abstract and a
						link to the full review. </documentation>
				</annotation>
				<complexType>
					<choice>
						<element ref="pmeta:ReviewCompleteText"/>
						<choice maxOccurs="2">
							<element ref="pmeta:ReviewIdentifier"/>
							<element ref="pmeta:ReviewExcerptText"/>
						</choice>
					</choice>
				</complexType>
			</element>
			<element ref="pmeta:ReviewContribution"/>
			<element name="ReviewPublicationDetails">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Provides publication time information for the review.
					</documentation>
				</annotation>
				<complexType>
					<sequence>
						<choice>
							<element ref="pmeta:FirstPublicationEventStart"/>
							<element ref="pmeta:PublicationEventStart"/>
						</choice>
						<element ref="pmeta:PublicationEventEnd" minOccurs="0"/>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="RightGrantOfRightsMediaType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies the type of media covered by the grant of rights
				through its code and/or name. </documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:RightGrantOfRightsMediaCode" minOccurs="0"/>
			<element ref="pmeta:RightGrantOfRightsMediaName" minOccurs="0"/>
		</choice>
	</complexType>
	<complexType name="RightTerritoryDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defines the territories covered by rights through a description
				or lists of covered or excluded territories. <H4>Example</H4>"the whole world,
				except United States", "Italy, except city of Rome" or codes. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:RightTerritoryDescription" minOccurs="0"/>
			<element ref="pmeta:CoveredTerritory" maxOccurs="unbounded"/>
			<element ref="pmeta:ExcludedTerritory" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	<complexType name="RightTypeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a type of right by its code or description.
			</documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:RightTypeCode"/>
			<element ref="pmeta:RightTypeDescription"/>
		</choice>
	</complexType>
	<complexType name="RoleTypeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a type of role by its code or name </documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:RoleTypeCode"/>
			<element ref="pmeta:RoleTypeName"/>
		</choice>
	</complexType>
	<complexType name="SeriesType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Describes a series as a self defined group of programmes.
			</documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:ProgrammeGroupIdentification"/>
			<element ref="pmeta:Identifier"/>
			<element name="Episode" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Provides additional information for each episode in the
						series. </documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:ProgrammeIdentification"/>
						<choice minOccurs="0">
							<element ref="pmeta:FirstPublicationEventStart"/>
							<element ref="pmeta:MaterialReleaseForPublicationDate"/>
						</choice>
						<element ref="pmeta:Duration" minOccurs="0"/>
						<element ref="pmeta:ExplanatoryNote" minOccurs="0"/>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="ServiceType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a service by its name and possibly its mother
				organisation name. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:OrganisationName" minOccurs="0"/>
			<element ref="pmeta:ServiceName"/>
		</sequence>
	</complexType>
	<complexType name="SignLanguageType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4> Identifies a sign language by its code and/or name.
			</documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:SignLanguageCode"/>
			<element ref="pmeta:SignLanguageName"/>
		</choice>
	</complexType>
	<complexType name="SignLanguageDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Provides information on sign languages associated to the
				content. </documentation>
		</annotation>
		<sequence>
			<element name="SignLanguage" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Identifies and describes a sign language.
					</documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:SignLanguage"/>
						<element ref="pmeta:SignLanguagePrimaryLanguageIndicator" minOccurs="0"/>
						<element ref="pmeta:SignLanguageTranslationIndicator" minOccurs="0"/>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="SIUnitOfMeasureType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a unit of measure by its code or name.
			</documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:SIUnitOfMeasureCode"/>
			<element ref="pmeta:SIUnitOfMeasureName"/>
		</choice>
	</complexType>
	<complexType name="StorageInstanceType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Communicates a basic set of information describing the storage
				instance containing the material. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:StorageType" minOccurs="0"/>
			<element ref="pmeta:StorageIdentifier"/>
		</sequence>
	</complexType>
	<complexType name="StorageTypeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a type of storage by its code or name
			</documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:StorageTypeCode"/>
			<element ref="pmeta:StorageTypeName"/>
		</choice>
	</complexType>
	<complexType name="TechnicalDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Communicates the minimum technical information required to allow
				the use of the instance of the material. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element ref="pmeta:AudioFormat" minOccurs="0"/>
			<element ref="pmeta:AudioCompression" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:AudioSampleRate" minOccurs="0"/>
			<element ref="pmeta:AudioBitrate" minOccurs="0"/>
			<element ref="pmeta:AudioBitsPerSample" minOccurs="0"/>
			<element ref="pmeta:AudioFixedBitrateIndicator" minOccurs="0"/>
			<element ref="pmeta:AudioReferenceLevel" minOccurs="0"/>
			<element ref="pmeta:AudioLoudness" minOccurs="0"/>
			<element ref="pmeta:AudioLoudnessRange" minOccurs="0"/>
			<element  ref="pmeta:AudioMaximumTruePeakLevel" minOccurs="0"/>
			<element ref="pmeta:VideoCompression" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="pmeta:VideoSampleRate" minOccurs="0"/>
			<element ref="pmeta:VideoSamplingHierarchyCode" minOccurs="0"/>
			<element ref="pmeta:VideoInterlacedSamplingStructureFlag" minOccurs="0"/>
			<element ref="pmeta:VideoTotalSamplesPerLine" minOccurs="0"/>
			<element ref="pmeta:VideoTotalLinesPerFrame" minOccurs="0"/>
			<element ref="pmeta:VideoFixedBitrateIndicator" minOccurs="0"/>
			<element ref="pmeta:VideoFrameRate" minOccurs="0"/>
			<element ref="pmeta:VideoBitsPerPixel" minOccurs="0"/>
			<element ref="pmeta:VideoAverageBitrate" minOccurs="0"/>
			<element ref="pmeta:VideoActiveSamplesPerLine" minOccurs="0"/>
			<element ref="pmeta:VideoActiveLinesPerFrame" minOccurs="0"/>
			<element ref="pmeta:PictureOriginalFramingAspectRatio" minOccurs="0"/>
			<element ref="pmeta:PictureIntendedDisplayAspectRatio" minOccurs="0"/>
			<element ref="pmeta:PictureDisplayFormatCode" minOccurs="0"/>
			<element ref="pmeta:PictureActionHorizontalSafePercentage" minOccurs="0"/>
			<element ref="pmeta:PictureActionVerticalSafePercentage" minOccurs="0"/>
			<element ref="pmeta:PictureGraphicsHorizontalSafePercentage" minOccurs="0"/>
			<element ref="pmeta:PictureGraphicsVerticalSafePercentage" minOccurs="0"/>
			<element ref="pmeta:DataCompression" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	<complexType name="TerritoryType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies a territory by its code or name. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:TerritoryCodeScheme"/>
			<choice maxOccurs="2">
				<element ref="pmeta:TerritoryCode"/>
				<element ref="pmeta:TerritoryName"/>
			</choice>
		</sequence>
	</complexType>
	<complexType name="TextUsageTypeType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Identifies by a code and/or a name a type of usage for text
				material </documentation>
		</annotation>
		<choice maxOccurs="2">
			<element ref="pmeta:TextUsageTypeCode"/>
			<element ref="pmeta:TextUsageTypeName"/>
		</choice>
	</complexType>
	<complexType name="TimestampType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defines a reference point in time by a date and / or time.
			</documentation>
		</annotation>
		<sequence>
			<element name="Date" type="date"/>
			<element name="Time" type="time" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="TitleHistoryType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Communicates the different titles associated with the material.
			</documentation>
		</annotation>
		<choice>
			<element ref="pmeta:BrandTitleHistory"/>
			<element ref="pmeta:ProgrammeGroupTitleHistory"/>
			<element ref="pmeta:ProgrammeTitleHistory"/>
			<element ref="pmeta:ItemTitleHistory"/>
			<element ref="pmeta:MediaObjectTitleHistory"/>
		</choice>
	</complexType>
	<complexType name="TransmissionPublicationDetailsType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Communicates the basic information identifying individual
				broadcast publication events known for the material. The live transmission flag is
				only relevant in the case of a first transmission. Optionally, for each publication
				event, information about the audience scores may be given. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:DataDefaultLanguage" minOccurs="0"/>
			<element name="PublicationDetails" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Provides detailed information for each transmission
						individually </documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:PublicationEventTransmissionService" minOccurs="0"/>
						<choice>
							<sequence>
								<element ref="pmeta:FirstPublicationEventStart"/>
								<element ref="pmeta:PublicationEventLiveTransmissionFlag"
									minOccurs="0"/>
							</sequence>
							<element ref="pmeta:PublicationEventStart"/>
						</choice>
						<element ref="pmeta:PublicationEventEnd" minOccurs="0"/>
						<element name="AudienceScoreDetails" minOccurs="0" maxOccurs="unbounded">
							<annotation>
								<documentation xml:lang="EN">
									<H4>Description</H4>Provides audience scores for each
									transmission. </documentation>
							</annotation>
							<complexType>
								<sequence>
									<element ref="pmeta:OrganisationDetails" minOccurs="0"/>
									<element ref="pmeta:AudienceScoreRecordingTechnique"
										minOccurs="0"/>
									<element name="AudienceScore">
										<complexType>
											<choice maxOccurs="3">
												<element ref="pmeta:AudienceRatingPercentage"/>
												<element ref="pmeta:AudienceReachPercentage"/>
												<element ref="pmeta:AudienceSharePercentage"/>
											</choice>
										</complexType>
									</element>
								</sequence>
							</complexType>
						</element>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="TransmissionPublicationSummaryType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Communicates the number of complete broadcast publications of
				the material on the specified transmission channels. </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:TotalBroadcastTransmissionsCount" minOccurs="0"/>
			<element name="TransmissionsPerService" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="EN">
						<H4>Description</H4>Provides information on broadcast services upon which
						the these transmissions have been divided and their respective count.
					</documentation>
				</annotation>
				<complexType>
					<sequence>
						<element ref="pmeta:PublicationEventTransmissionService" minOccurs="0"/>
						<element ref="pmeta:PublicationEventCount" minOccurs="0"/>
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="ValidityWindowType">
		<annotation>
			<documentation xml:lang="EN">
				<H4>Description</H4>Defines a time window of validity. <H4>Aliases</H4>Validity
				period </documentation>
		</annotation>
		<sequence>
			<element ref="pmeta:ValidFrom" minOccurs="0"/>
			<element ref="pmeta:ValidTo" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="VideoCompressionType">
		<annotation>
			<documentation xml:lang="EN">
